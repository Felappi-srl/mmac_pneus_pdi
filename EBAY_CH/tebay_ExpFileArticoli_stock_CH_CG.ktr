, case when codca5 is not null then desca5 else '' end as ivel
, case when codca6 is not null then desca6 else '' end as icarico
, case when codca2 is not null then DESca2 else '' end as offset
, case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'PNEUMATICI' end as category_path
,product_full_image
, codca2 as tipoutilizzo
FROM linky_ebayarticoli as a inner JOIN linky_ebay_report on a.codice = linky_ebay_report.SKU and 'CH' = linky_ebay_report.SITEID
--and a.product_in_stock&lt;&gt;linky_ebay_report.qty
