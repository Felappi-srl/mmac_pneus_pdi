    <connection>LINKY</connection>
    <sql>SELECT distinct
 codice as product_sku
, substring(replace(short_desc , 'GOMME PNEUMATICI','TYRE') + 
case when codca2 in ('X','N','R') then ' ' + REPLACE(desca2,'INVERNALI','WINTER') else '' end,1,80)
as Titolo
, cast (ebaycat as varchar) as ebaycat
,'Switzerland' as siteid
,'Italy' as country
,'FixedPriceItem' as Type
,  Marca 
, dbo.linky_calc_endprice(partner,'CH',case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'PNEUMATICI' end,'',prezzolistino)  as product_price
, product_in_stock as product_in_stock
, case when codca1 is not null then codca1 else '' end as diametro
, case when codca3 is not null then codca3 else '' end as spalla
, case when codca4 is not null then codca4 else '' end as larghezza
, case when codca5 is not null then desca5 else '' end as ivel
, case when codca6 is not null then desca6 else '' end as icarico
, case when codca2 is not null then codca2 else '' end as offset
, case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'PNEUMATICI' end as category_path
,product_full_image
, codca2 as tipoutilizzo
FROM linky_ebayarticoli as a inner JOIN linky_ebay_report on a.codice = linky_ebay_report.SKU and a.siteid = linky_ebay_report.SITEID
--and a.product_in_stock&lt;&gt;linky_ebay_report.qty
where product_in_stock > 0 
and pubblicato='Y' 
and
a.siteid='CH' and 
-- codca2='R' and 
--codice='1840235VWRA3XL' and
category_path not in ('CERCHI IN LEGA','ACCOPPIATE MOTO','SCOOTER','MOTO', 'CATENE DA NEVE','')
 and (partner in (SELECT CODICEPARTNER FROM LINKY_PARTNERS where TIPO='PNEUMATICI') or partner='PNS')
-- procedura fatta per non aggiornare le inserzioni con prezzo sbagliato
and prezzolistino > 20
</sql>
