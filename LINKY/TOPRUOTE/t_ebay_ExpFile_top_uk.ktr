    <connection>LINKY</connection>
    <sql>SELECT distinct
 codicevar as product_sku
, 
    substring('ALLOY WHEEL ' + Marca + ' ' + var.short_desc,1,76) + ' ' + SUBSTRING(CONVERT(varchar(40), NEWID()),0,4) 
as Titolo
,'ALLOY WHEEL ' +  var.short_desc as titolo_header
, ebaycat
,'UK' as siteid
,'IT' as country
,'FixedPriceItem' as Type
,  Marca 
, dbo.linky_calc_endprice(partner,'UK','CERCHI','',e.prezzo)  as product_price
, product_in_stock as product_in_stock
, case when codca1 is not null then codca1 else '' end as diametro
, case when codca3 is not null then codca3 else '' end as spalla
, case when codca4 is not null then desca4 else '' end as larghezza
, case when codca5 is not null then desca5 else '' end as ivel
, case when codca6 is not null then desca6 else '' end as icarico
, case when codca2 is not null then DESca2 else '' end as offset
, case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'PNEUMATICI' end as category_path
,product_full_image
, codca2 as tipoutilizzo
, MARCAAUTO
, MODELLOAUTO
, coalesce(ANNOAUTO,'') annoauto
, partner,
case when urlcertificato is not null then
'&lt;p>&lt;a href="' + urlcertificato + '" target="_blank">Download Certificate&lt;/a>&lt;/p>'
else
''
end as urlcertificato
,EANCODE,
var.codice as mpn
FROM linky_ebay e inner join linky_ebay_var var on e.codice=var.codice
LEFT JOIN linky_ebay_report on var.codicevar = linky_ebay_report.SKU and 'UK' = linky_ebay_report.SITEID
where 
 linky_ebay_report.SKU is null and
 product_in_stock > 3  and PARTNER ='TOP' AND prezzolistino > 10 AND
(var.siteid='United Kingdom' or var.siteid='UK') and 
--codice='1840235VWRA3XL' and
--codicevar ='CL00000267843MERCEDESBENZBCLASS2005245' and
category_path  in ('CERCHI IN LEGA')
--and lastupdate is not null
and (out_prod&lt;&gt;'1' OR out_prod IS NULL)</sql>
