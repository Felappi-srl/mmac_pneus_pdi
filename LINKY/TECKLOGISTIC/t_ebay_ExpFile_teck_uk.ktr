  <step>
    <name>CERCHI LEGA TECK</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>LINKY</connection>
    <sql>SELECT distinct
 codicevar as product_sku
,upper( substring(replace(
 var.short_desc,'CERCHI IN LEGA','ALLOY WHEEL'),1,76) + ' ' + SUBSTRING(CONVERT(varchar(40), NEWID()),0,4) )
as Titolo,
 replace(replace( var.short_desc,'.00','') ,'CERCHI IN LEGA','ALLOY WHEEL')
as titolo_header
, ebaycat
,'UK' as siteid
,'IT' as country
,'FixedPriceItem' as Type
,  Marca 
, dbo.linky_calc_endprice(partner,'UK','CERCHI','',prezzolistino)  as product_price
, product_in_stock as product_in_stock
, case when codca1 is not null then codca1 else '' end as diametro
, replace(case when codca3 is not null then codca3 else '' end,'.00','') as spalla
, replace(case when codca4 is not null then desca4 else '' end,'.00','') as larghezza
, case when codca5 is not null then desca5 else '' end as ivel
, case when codca6 is not null then desca6 else '' end as icarico
, case when codca2 is not null then DESca2 else '' end as offset
, case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'PNEUMATICI' end as category_path
,product_full_image
, codca2 as tipoutilizzo
, MARCAAUTO
, MODELLOAUTO
, coalesce(ANNOAUTO,'') annoauto
, partner
, case when urlimgauto is not null then
	'&lt;p>&lt;img src="' + replace(urlimgauto,'viewer?','viewerimage.php?') + '" width="640px" height="300px">&lt;/p>'
else
	''
end as urlimgauto
, case when urlcertificato is not null then
'&lt;p>&lt;a href="' + urlcertificato + '" target="_blank">Herunterladen des Zertifikats&lt;/a>&lt;/p>'
else
''
end as urlcertificato,eancode,
var.codice as mpn

FROM linky_ebay e inner join linky_ebay_var var on e.codice=var.codice
LEFT JOIN linky_ebay_report on var.codicevar = linky_ebay_report.SKU and 'UK' = linky_ebay_report.SITEID
where 
 linky_ebay_report.SKU is null and
 product_in_stock > 1  and PARTNER ='TECK' AND prezzolistino > 10 AND
(var.siteid='United Kingdom' or var.siteid='UK') and 
--codice='1840235VWRA3XL' and
category_path  in ('CERCHI IN LEGA')
--and lastupdate is not null</sql>
