  <step>
    <name>CERCHI LEGA PNS</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>LINKY</connection>
    <sql>SELECT distinct
 e.codice as product_sku
, substring('ALLOY WHEEL ' + e.marca + ' ' + whl.WHEELMODEL + ' ' + replace(replace(REPLACE(whl.CANALE, ',0', ''),',', '.'), 'j','') + 'x' + whl.RAGGIO + ' ' + COALESCE( whl.NUMEROFORI + 'x' + whl.INTERASSE, whl.PCD) + ' ET' + whl.OFFSET + ' ' + whl.FINISHING ,1,76) 
as Titolo

,'ALLOY WHEEL ' + e.marca + ' ' + whl.WHEELMODEL + ' ' + replace(replace(REPLACE(whl.CANALE, ',0', ''),',', '.'), 'j','') + 'x' + whl.RAGGIO + ' ' + COALESCE( whl.NUMEROFORI + 'x' + whl.INTERASSE, whl.PCD) + ' ET' + whl.OFFSET + ' ' + whl.FINISHING as titolo_header
, dbo.linky_category_ebay ('US','CERCHI') as ebaycat
,'US' as siteid
,'IT' as country
,'FixedPriceItem' as Type
,  Marca 
, dbo.linky_calc_endprice(partner,'US','CERCHI','',coalesce(e.prezzopubblico,0))  as product_price
, product_in_stock
, case when codca1 is not null then codca1 else '' end as diametro
, case when codca3 is not null then codca3 else '' end as spalla
, case when codca4 is not null then desca4 else '' end as larghezza
, case when codca5 is not null then desca5 else '' end as ivel
, case when codca6 is not null then desca6 else '' end as icarico
, case when codca2 is not null then desca2 else '' end as offset
, case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'PNEUMATICI' end as category_path
,product_full_image
, codca2 as tipoutilizzo
, '' as MARCAAUTO
, '' as MODELLOAUTO
, '' as annoauto
FROM linky_ebay e inner join connector.dbo.linky_articoli_PNS as whl on e.codice= whl.PARTNUMBER
INNER JOIN linky_ebay_report as rep on e.codice = rep.SKU and 'US' = rep.SITEID
where 
PARTNER ='PNS' AND
product_in_stock > 3  and 
(rep.qty&lt;&gt;e.product_in_stock or rep.prezzo &lt;&gt; dbo.linky_calc_endprice(partner,'US','CERCHI','',coalesce(e.prezzopubblico,0)) )and
rep.siteid='US'
</sql>
