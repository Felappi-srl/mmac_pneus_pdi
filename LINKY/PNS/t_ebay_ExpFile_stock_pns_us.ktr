    </partitioning>
    <connection>LINKY</connection>
    <sql>SELECT distinct
 codice as product_sku
,'US' as siteid
,'IT' as country
FROM linky_ebay e inner join connector.dbo.linky_articoli_PNS as whl on e.codice= whl.PARTNUMBER
INNER JOIN linky_ebay_report as rep on e.codice = rep.SKU and 'US' = rep.SITEID
where 
(product_in_stock &lt; 4 )
-- or pubblicato='N') 
and 
PARTNER ='PNS' AND
(rep.siteid='US') 
--and 
--category_path  in ('CERCHI IN LEGA')
and codice is not null

ORDER BY product_sku asc
--and lastupdate is not null</sql>
    <limit>0</limit>
    <lookup/>
