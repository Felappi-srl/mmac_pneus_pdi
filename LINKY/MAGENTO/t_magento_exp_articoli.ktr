    <sql>SELECT distinct&#xd;&#xa; codca1 as pn_raggio&#xd;&#xa;, codca3 as pn_spalla&#xd;&#xa;, codca4 as pn_l_battistrada&#xd;&#xa;, codca6 as pn_i_carico&#xd;&#xa;, desca5 as pn_i_velocita&#xd;&#xa;, case when desca2 is null then codca2 else desca2 end as pn_stagione&#xd;&#xa;, rotolamento as pn_risparmio_carburante&#xd;&#xa;, ADERENZA as pn_aderenza&#xd;&#xa;, RUMOROSITA as pn_rumore&#xd;&#xa;, &#x27;admin&#x27; as store&#xd;&#xa;, &#x27;base&#x27; as websites&#xd;&#xa;, &#x27;Pneumatici&#x27; as attribute_set&#xd;&#xa;,&#x27;simple&#x27; as type&#xd;&#xa;, codice as sku&#xd;&#xa;, nome as name&#xd;&#xa;, short_desc  as short_description&#xd;&#xa;, descrizione as description&#xd;&#xa;, case when product_publish&#x3d;&#x27;Y&#x27; then &#x27;1&#x27; else &#x27;0&#x27; end as status&#xd;&#xa;, &#x27;Taxable Goods&#x27; as tax_class_id&#xd;&#xa;,  replace&#x28;replace&#x28;replace&#x28;replace&#x28;category_path ,&#x27;VETTURA&#x27;,&#x27;AUTO&#x27;&#x29;,&#x27;FUORISTRADA&#x27;,&#x27;AUTO&#x27;&#x29;,&#x27;TRASPORTO LEGGERO&#x27;,&#x27;AUTO&#x27;&#x29;,&#x27;&#x23;&#x27;,&#x27;&#x2f;&#x27;&#x29; as categories&#xd;&#xa;,&#x27;1&#x27; as use_external_images&#xd;&#xa;, linky_articoli.product_in_stock as qty&#xd;&#xa;, case when  linky_articoli.product_in_stock&#x3e;0 then &#x27;1&#x27; else &#x27;0&#x27; end as is_in_stock&#xd;&#xa;,  Marca as manufacturer&#xd;&#xa;,  Marca as vesbrand&#xd;&#xa;, case when prezzolistino &#x3c; 100 then&#xd;&#xa;         round&#x28;round&#x28;cast&#x28;prezzolistino as numeric&#x28;15,5&#x29;&#x29;&#x2f;1.02,0&#x29;&#x2f;1.22,2&#x29; &#xd;&#xa;  else&#xd;&#xa;         round&#x28;round&#x28;cast&#x28;prezzolistino as numeric&#x28;15,5&#x29;&#x29;&#x2f;1.02,0&#x29;&#x2f;1.22,2&#x29; &#xd;&#xa;  end&#xd;&#xa;as price&#xd;&#xa;, replace&#x28;product_full_image,&#x27;images&#x2f;stories&#x2f;virtuemart&#x2f;product&#x2f;&#x27;,&#x27;http&#x3a;&#x2f;&#x2f;s3-eu-west-1.amazonaws.com&#x2f;cdn.pneus-center.it&#x2f;web&#x2f;img&#x2f;&#x27;&#x29; as image_external_url&#xd;&#xa;, replace&#x28;product_full_image,&#x27;images&#x2f;stories&#x2f;virtuemart&#x2f;product&#x2f;&#x27;,&#x27;http&#x3a;&#x2f;&#x2f;s3-eu-west-1.amazonaws.com&#x2f;cdn.pneus-center.it&#x2f;web&#x2f;img&#x2f;resized&#x2f;&#x27;&#x29; as thumbnail_external_url&#xd;&#xa;, replace&#x28;product_full_image,&#x27;images&#x2f;stories&#x2f;virtuemart&#x2f;product&#x2f;&#x27;,&#x27;http&#x3a;&#x2f;&#x2f;s3-eu-west-1.amazonaws.com&#x2f;cdn.pneus-center.it&#x2f;web&#x2f;img&#x2f;resized&#x2f;&#x27;&#x29; as small_image_external_url&#xd;&#xa;, &#x27;1&#x27; as weight&#xd;&#xa;FROM linky_articoli &#xd;&#xa;where linky_articoli.product_in_stock &#x3e; 0 and prezzolistino &#x3e; 0&#xd;&#xa;and nome not like &#x27;ACCOPPIATA&#x25;&#x27;&#xd;&#xa;and nome not like &#x27;CATENE&#x25;&#x27;&#xd;&#xa;and full_category_path not like &#x27;CERCHI&#x25;&#x27; </sql>
