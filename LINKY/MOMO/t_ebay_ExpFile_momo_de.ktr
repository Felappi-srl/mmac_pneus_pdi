    <sql>SELECT distinct &#xa; codicevar as product_sku&#xa;, &#xd;&#xa;    substring&#x28;&#x27;ALUFELGE &#x27; &#x2b; Marca &#x2b; &#x27; &#x27; &#x2b; var.short_desc,1,76&#x29; &#x2b; &#x27; &#x27; &#x2b; SUBSTRING&#x28;CONVERT&#x28;varchar&#x28;40&#x29;, NEWID&#x28;&#x29;&#x29;,0,4&#x29; &#xd;&#xa;as Titolo&#xd;&#xa;,&#x27;ALUFELGE MOMO &#x27; &#x2b; var.short_desc as titolo_header&#xa;, ebaycat&#xa;,&#x27;Germany&#x27; as siteid&#xa;,&#x27;IT&#x27; as country&#xa;,&#x27;FixedPriceItem&#x27; as Type&#xa;,  Marca &#xa;, dbo.linky_calc_endprice&#x28;partner,&#x27;DE&#x27;,&#x27;CERCHI&#x27;,&#x27;&#x27;,prezzolistino&#x29;  as product_price&#xd;&#xa;, product_in_stock as product_in_stock&#xa;, case when codca1 is not null then codca1 else &#x27;&#x27; end as diametro&#xa;, case when codca3 is not null then codca3 else &#x27;&#x27; end as spalla&#xa;, case when codca4 is not null then desca4 else &#x27;&#x27; end as larghezza&#xa;, case when codca5 is not null then desca5 else &#x27;&#x27; end as ivel&#xa;, case when codca6 is not null then desca6 else &#x27;&#x27; end as icarico&#xa;, case when codca2 is not null then DESca2 else &#x27;&#x27; end as offset&#xa;, case when category_path &#x3d; &#x27;CERCHI IN LEGA&#x27; then &#x27;CERCHI&#x27; else &#x27;PNEUMATICI&#x27; end as category_path&#xa;,product_full_image&#xd;&#xa;, codca2 as tipoutilizzo&#xd;&#xa;, MARCAAUTO&#xd;&#xa;, MODELLOAUTO&#xd;&#xa;, coalesce&#x28;ANNOAUTO,&#x27;&#x27;&#x29; annoauto&#xd;&#xa;, partner,&#xd;&#xa;&#xd;&#xa;&#x27;&#x27; as urlcertificato&#xd;&#xa;,EANCODE,&#xd;&#xa;var.codice as mpn&#xa;FROM linky_ebay e inner join linky_ebay_var var on e.codice&#x3d;var.codice&#xa;LEFT JOIN linky_ebay_report on REPLACE&#x28;REPLACE&#x28;var.codicevar,CHAR&#x28;10&#x29;,&#x27;&#x27;&#x29;,CHAR&#x28;13&#x29;,&#x27;&#x27;&#x29; &#x3d; linky_ebay_report.SKU and &#x27;DE&#x27; &#x3d; linky_ebay_report.SITEID&#xd;&#xa;where &#xd;&#xa; linky_ebay_report.SKU is null and var.istuv&#x3d;&#x27;1&#x27; and&#xd;&#xa; product_in_stock &#x3e; 3  and PARTNER &#x3d;&#x27;MOMO&#x27; AND prezzolistino &#x3e; 10 AND&#xd;&#xa;&#x28;var.siteid&#x3d;&#x27;Germany&#x27; or var.siteid&#x3d;&#x27;DE&#x27;&#x29; and &#xd;&#xa;--codice&#x3d;&#x27;1840235VWRA3XL&#x27; and&#xa;category_path  in &#x28;&#x27;CERCHI IN LEGA&#x27;&#x29;&#xd;&#xa;--and lastupdate is not null&#xa;</sql>
