  <step>
    <name>ACCOPPIATE</name>
    <type>TableInput</type>
    <description/>
    <distribute>N</distribute>
    <custom_distribution/>
    <copies>1</copies>
    <partitioning>
      <method>none</method>
      <schema_name/>
    </partitioning>
    <connection>LINKY</connection>
    <sql>SELECT distinct
 codice as product_sku
, substring(rtrim(replace(short_desc,'ACCOPPIATA GOMME','MOTORRADREIFEN ') + 
case when codca2 in ('X','N','R') then ' ' + desca2 else '' end),1,76) + ' ' + SUBSTRING(CONVERT(varchar(40), NEWID()),0,4)   as Titolo
, CAST (ebaycat as VARCHAR) as ebaycat
,'Switzerland' as siteid
,'CH' as country
,'FixedPriceItem' as Type
,  Marca 
, dbo.linky_calc_endprice(partner,'CH',case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'ACCOPPIATE MOTO' end,'',prezzolistino)  as product_price
, product_in_stock as product_in_stock
, case when codca1 is not null then desca1 else '' end as diametro
, case when codca3 is not null then codca3 else '' end as spalla
, case when codca4 is not null then codca4 else '' end as larghezza
, case when codca5 is not null then codca5 else '' end as tmp_ivel
, case when codca6 is not null then desca6 else '' end as icarico
, case when codca2 is not null then codca2 else '' end as offset
, case when category_path = 'CERCHI IN LEGA' then 'CERCHI' 
else 'ACCOPPIATE MOTO' end as category_path
,product_full_image
, codca2 as tipoutilizzo
,'' as energy
, '' as rumorosita
, '' as codicerotolamento
, '' as codiceaderenza
, partner
,eancode,'' as runflat,category_path as tipop
FROM linky_ebayarticoli 
LEFT JOIN linky_ebay_report on linky_ebayarticoli.codice = linky_ebay_report.SKU and 'CH'= linky_ebay_report.SITEID
where linky_ebay_report.SKU is null and
 product_in_stock > 0 and  
pubblicato='Y' and
linky_ebayarticoli.siteid='CH' and 
--codiceN' and
category_path  in ('ACCOPPIATE MOTO')
-- and 1=2</sql>
