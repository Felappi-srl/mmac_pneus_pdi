    <connection>LINKY</connection>
    <sql>SELECT distinct
 codicevar as product_sku
, substring(replace(replace(rtrim(var.short_desc),'CATENE NEVE','SCHNEEKETTEN'),'CATENE DA NEVE','SCHNEEKETTEN') +  case when codca5 is not null then ' ' + rtrim(ltrim(desca5)) + ' mm DICKE' else '' end  , 1,76) + ' ' + SUBSTRING(CONVERT(varchar(40), NEWID()),0,4) 
as Titolo
, '40278' as ebaycat
,'Switzerland' as siteid
,'CH' as country
,'FixedPriceItem' as Type
,  Marca 
, dbo.linky_calc_endprice(partner,'CH',case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'CATENE DA NEVE' end,'',prezzolistino)  as product_price
, product_in_stock as product_in_stock
, case when codca1 is not null then codca1 else '' end as diametro
, case when codca3 is not null then codca3 else '' end as spalla
, case when codca4 is not null then codca4 else '' end as larghezza
, case when codca5 is not null then rtrim(ltrim(desca5)) + ' mm' else '' end as ivel
, case when codca6 is not null then desca6 else '' end as icarico
, case when codca2 is not null then codca2 else '' end as offset
, case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'CATENE DA NEVE' end as category_path
,product_full_image
, codca2 as tipoutilizzo
,'' as energy
, '' as rumorosita
, '' as codicerotolamento
, '' as codiceaderenza
, partner
,eancode,'' as runflat,category_path as tipop
FROM linky_ebay e inner join linky_ebay_var var on e.codice=var.codice
LEFT JOIN linky_ebay_report on var.codicevar = linky_ebay_report.SKU and var.siteid= linky_ebay_report.SITEID
where linky_ebay_report.SKU is null and
 product_in_stock > 0 and 
pubblicato='Y' and
var.siteid='CH' and 
-- codca2='R' and 
--e.codice='CATWEIM90GRF30' and
category_path in ( 'CATENE DA NEVE')
-- and 1=2
union all
SELECT distinct
 codice as product_sku
, substring(replace(replace(rtrim(e.short_desc),'CATENE DA NEVE','SNOW CHAINS'),'CATENE NEVE','SNOW CHAINS') +  case when codca5 is not null then ' ' + ltrim(rtrim(desca5)) + ' mm THICKNESS' else '' end , 1,76) + ' ' + SUBSTRING(CONVERT(varchar(40), NEWID()),0,4) 
as Titolo
, ebaycat
,'Switzerland' as siteid
,'CH' as country
,'FixedPriceItem' as Type
,  Marca 
, dbo.linky_calc_endprice(partner,'UK',case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'CATENE DA NEVE' end,'',prezzolistino)  as product_price
, product_in_stock as product_in_stock
, case when codca1 is not null then codca1 else '' end as diametro
, case when codca3 is not null then codca3 else '' end as spalla
, case when codca4 is not null then codca4 else '' end as larghezza
, case when codca5 &lt;&gt;'0'then rtrim(ltrim(desca5)) + ' mm' else '' end as ivel
, case when codca6 is not null then desca6 else '' end as icarico
, case when codca2 is not null then codca2 else '' end as offset
, case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'CATENE DA NEVE' end as category_path
,product_full_image
, codca2 as tipoutilizzo
,'' as energy
, '' as rumorosita
, '' as codicerotolamento
, '' as codiceaderenza
, partner,
eancode,'' as runflat,category_path as tipop
FROM linky_ebayarticoli e
LEFT JOIN linky_ebay_report on e.codice = linky_ebay_report.SKU and e.siteid = linky_ebay_report.SITEID
where 
 linky_ebay_report.SKU is null and
product_in_stock > 0 and 
pubblicato='Y' and
e.siteid='CH' and 
-- codca2='R' and
--codice like 'CATPEWRSS73%' and
category_path in ( 'CATENE DA NEVE')
-- and 1=2</sql>
