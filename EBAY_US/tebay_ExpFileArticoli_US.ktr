    <connection>LINKY</connection>
    <sql>SELECT distinct
 codice as product_sku
, substring(replace(short_desc,'ACCOPPIATA GOMME','TIRE SET') + 
case when codca2 in ('X','N','R') then ' ' + desca2 else '' end,1,76) + ' ' + SUBSTRING(CONVERT(varchar(40), NEWID()),0,4)   as Titolo
, ebaycat
,'US' as siteid
,'IT' as country
,'FixedPriceItem' as Type
,  Marca 
, dbo.linky_calc_endprice(partner,'US',case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'ACCOPPIATE MOTO' end,'',prezzolistino)  as product_price
, product_in_stock as product_in_stock
, case when codca1 is not null then desca1 else '' end as diametro
, case when codca3 is not null then codca3 else '' end as spalla
, case when codca4 is not null then codca4 else '' end as larghezza
, case when codca5 is not null then desca5 else '' end as ivel
, case when codca6 is not null then desca6 else '' end as icarico
, case when codca2 is not null then codca2 else '' end as offset
, case when category_path = 'CERCHI IN LEGA' then 'CERCHI' 
else 'ACCOPPIATE MOTO' end as category_path
,product_full_image
, codca2 as tipoutilizzo
,'' as energy
, '' as rumorosita
, '' as codicerotolamento
, '' as codiceaderenza
, partner
,eancode
FROM linky_ebayarticoli 
LEFT JOIN linky_ebay_report on linky_ebayarticoli.codice = linky_ebay_report.SKU and 'US'= linky_ebay_report.SITEID
where linky_ebay_report.SKU is null and
 product_in_stock > 0 and  
pubblicato='Y' and
linky_ebayarticoli.siteid='US' and 
--codiceN' and
category_path  in ('ACCOPPIATE MOTO')
</sql>
