    </partitioning>
    <connection>LINKY</connection>
    <sql>SELECT distinct
 codice as product_sku
, substring(replace(short_desc , 'GOMME PNEUMATICI','TYRES') + 
case when codca2 in ('X','N','R') then ' ' + REPLACE(desca2,'INVERNALI','WINTER') else '' end,1,76) + ' ' + SUBSTRING(CONVERT(varchar(40), NEWID()),0,4) 
as Titolo
, ebaycat
,'UK' as siteid
,'IT' as country
,'FixedPriceItem' as Type
,  Marca 
, dbo.linky_calc_endprice(partner,'UK',case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'PNEUMATICI' end,'',prezzolistino)  as product_price
, product_in_stock as product_in_stock
, case when codca1 is not null then codca1 else '' end as diametro
, case when codca3 is not null then codca3 else '' end as spalla
, case when codca4 is not null then codca4 else '' end as larghezza
, case when codca5 is not null then desca5 else '' end as ivel
, case when codca6 is not null then desca6 else '' end as icarico
, case when codca2 is not null then codca2 else '' end as offset
, case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'PNEUMATICI' end as category_path
,product_full_image
, codca2 as tipoutilizzo
, case when codicerotolamento is not null and len(codicerumorosita) > 2  then
'&lt;p>&lt;img src="http://www.etichettepneumatici.it/Image/GetImage?fuelefficiency=' + 
 codicerotolamento + '&amp;wetgrip=' + codiceaderenza + '&amp;noiseclass=' + 
substring(codicerumorosita,charindex('_',codicerumorosita)+1,1) + 
'&amp;decibels=' + substring(codicerumorosita,1, charindex('_',codicerumorosita)-1) + '&amp;width=300" alt="" />&lt;/p>' 
else
''
end as energy
, case when len(codicerumorosita) > 2 then substring(codicerumorosita,1, charindex('_',codicerumorosita)-1) else '' end as rumorosita
, codicerotolamento
, codiceaderenza
, partner
,eancode
FROM linky_ebayarticoli 
LEFT JOIN linky_ebay_report on linky_ebayarticoli.codice = linky_ebay_report.SKU and 'UK'= linky_ebay_report.SITEID
where linky_ebay_report.SKU is null and
 product_in_stock > 0 and 
pubblicato='Y' and
linky_ebayarticoli.siteid='UK' and 
-- codca2='R' and 
--codice='1840235VWRA3XL' and
1=2 and
category_path not in ('CERCHI IN METALLO','CERCHI IN LEGA','ACCOPPIATE MOTO','SCOOTER','MOTO', 'CATENE DA NEVE','RUOTINI','')
</sql>
