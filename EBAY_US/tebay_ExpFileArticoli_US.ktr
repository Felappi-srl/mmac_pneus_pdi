    <connection>LINKY</connection>
    <sql>SELECT distinct
 codice as product_sku
, substring(replace(replace(short_desc,'RUOTINO DI SCORTA','SPARE WHEEL'),' PER ',' FOR ')  + 
case when codca2 in ('X','N','R') then ' ' + desca2 else '' end,1,76) + ' ' + SUBSTRING(CONVERT(varchar(40), NEWID()),0,4) 
as Titolo
, ebaycat
,'US' as siteid
,'IT' as country
,'FixedPriceItem' as Type
,  Marca 
, dbo.linky_calc_endprice(partner,'US','RUOTINI','',prezzolistino)  as product_price
, product_in_stock as product_in_stock
, case when desca1 is not null then desca1 else '' end as diametro
, case when desca3 is not null then desca3 else '' end as spalla
, case when desca4 is not null then desca4 else '' end as larghezza
, case when desca5 is not null then desca5 else '' end as ivel
, case when desca6 is not null then desca6 else '' end as icarico
, case when desca2 is not null then desca2 else '' end as offset
, 'RUOTINI' as category_path
,product_full_image
, codca2 as tipoutilizzo
, case when codicerotolamento is not null then
'&lt;p>&lt;img src="http://www.etichettepneumatici.it/Image/GetImage?fuelefficiency=' + 
 codicerotolamento + '&amp;wetgrip=' + codiceaderenza + '&amp;noiseclass=' + 
substring(codicerumorosita,charindex('_',codicerumorosita)+1,1) + 
'&amp;decibels=' + substring(codicerumorosita,1, charindex('_',codicerumorosita)-1) + '&amp;width=300" alt="" />&lt;/p>' 
else
''
end as energy
, substring(codicerumorosita,1, charindex('_',codicerumorosita)-1) as rumorosita
, codicerotolamento
, codiceaderenza
, partner,
eancode
FROM linky_ebayarticoli 
LEFT JOIN linky_ebay_report on linky_ebayarticoli.codice = linky_ebay_report.SKU and 'US'= linky_ebay_report.SITEID
where linky_ebay_report.SKU is null and
product_in_stock > 0 and 
pubblicato='Y' and
 linky_ebayarticoli.siteid='US' and 
-- codca2='R' and 
--codice like '1598990%' and
--codicerotolamento is not null and
category_path = 'RUOTINI'
</sql>
