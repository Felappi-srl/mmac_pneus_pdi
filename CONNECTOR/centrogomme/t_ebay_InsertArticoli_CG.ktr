    <connection>LINKY</connection>
    <sql>select
  marca as marchio
, sku as codice
, 'GOMME PNEUMATICI ' + COALESCE(TITOLOALTERNATIVO,DESCRIZIONE,'') AS NOME
, ' GOMME PNEUMATICI ' + COALESCE(TITOLOALTERNATIVO,'') + ' ' 
  + coalesce(LARGHEZZA,'') + '/' + coalesce(SERIE,'') 
+
case when rtrim(SETTORE) in  ('SCOOTER','MOTO') then
	case when rtrim(coalesce(VEL,'')) = 'W' then
	' ZR' else
	' R' end
	else
 ' R' 
end + coalesce(CODICECERCHIO,'') + ' ' + ltrim(coalesce(replace(icarico,',0',''),'')) + replace(rtrim(substring(coalesce(VEL,''),1,2)),'Z','W') + ' ' + coalesce(marca,'')  AS SHORT_DESC_DB
, marca + ' GOMME PNEUMATICI ' + COALESCE(TITOLOALTERNATIVO,'') + ' ' 
  + coalesce(LARGHEZZA,'') + '/' + coalesce(SERIE,'') +
case when rtrim(SETTORE) in  ('SCOOTER','MOTO') then
	case when rtrim(coalesce(VEL,'')) = 'W' then
	' ZR' else
	' R' end
	else
 ' R' 
end + coalesce(CODICECERCHIO,'') + ' ' + ltrim(coalesce(replace(icarico,',0',''),'')) + replace(rtrim(substring(coalesce(VEL,''),1,2)),'Z','W')  AS descrizione_DB
, SETTORE as category_path
, '' as full_category_path
, prezzoscontato as prezzo
, prezzoscontato as prezzoLISTINO
, 'EUR' AS currency
, 'http://s3-eu-west-1.amazonaws.com/cdn.pneus-center.it/web/foto/' + ltrim(rtrim(lower(cast(immagine as varchar(max))))) + '.jpg' AS product_full_image 
, 'http://s3-eu-west-1.amazonaws.com/cdn.pneus-center.it/web/foto/resized/' + ltrim(rtrim(lower(cast(immagine as varchar(max))))) + '.jpg' AS product_thumb_image 
, 'Y' AS product_publish
, '21' AS iva
, giacenza AS product_in_stock
, '12234563' AS product_available 
, 'N' AS product_special 
, CODICECERCHIO AS CODCA1
, CODICECERCHIO AS DESCA1
, CASE WHEN CODICESTAGIONE='I' THEN 'N'
 WHEN CODICESTAGIONE='4' THEN '4' 
ELSE 'E' END AS CODCA2
, CASE WHEN CODICESTAGIONE='I' THEN 'INVERNALI' 
 WHEN CODICESTAGIONE='4' THEN '4 STAGIONI'
ELSE 'ESTIVE' END AS DESCA2
, SERIE AS CODCA3
, 'SERIE ' + SERIE AS DESCA3
, LARGHEZZA AS CODCA4
, LARGHEZZA AS DESCA4
, replace(SUBSTRING(VEL,1,1),'Z','W') AS CODCA5_DB
, replace(SUBSTRING(VEL,1,1),'Z','W') +': ' +SUBSTRING(VEL,2,4) + ' Km/h' AS DESCA5
, ltrim(coalesce(replace(icarico,',0',''),'')) AS CODCA6
, ltrim(coalesce(replace(icarico,',0',''),'')) AS DESCA6
, 'Y' as pubblicato
, replace(cast(ebaycat as varchar(max)),' ','') as altreCatEbay
, partner
, codicerotolamento
, codiceaderenza
, codicerumorosita
, ean
, runflat
, radiale
, TITOLOALTERNATIVO
, LARGHEZZA
, SERIE
, CODICECERCHIO as DIAMETRO
, icarico as z_icarico
from linky_integro_articoli
WHERE IMMAGINE IS NOT NULL and immagine &lt;&gt; '' and ebaycat&lt;&gt;'' and bestprice=1
--and ipcodeSTD not in (select codice from linky_ebayarticoli)
--and marca='YOKOHAMA'
--and ipcode='3446053'
--ipcodestd IN ('2207900','1534800','2491500','2681200','2670700' , '630173', '2583600', '1816900', '2715900')
--order by ipcode ASC</sql>
