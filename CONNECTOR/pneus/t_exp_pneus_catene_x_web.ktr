    <connection>INPUT</connection>
    <sql>SELECT MSGRUMER.GruDes AS Marchio, &#xd;&#xa;	rtrim&#x28;ltrim&#x28;MZANAART.ArtCodice&#x29;&#x29; AS Cod, &#xd;&#xa;	MZANAART.ArtDescri AS Nome, &#xd;&#xa;	rtrim&#x28;MSGRUMER.GruDes&#x29; &#x2b;&#x27; &#x27; &#x2b; rtrim&#x28;MZANAART.ArtDescri&#x29; &#x2b; &#x27; &#x27; &#x2b; rtrim&#x28;MZANAART.ArtCodRep&#x29;  AS short ,&#xd;&#xa;	rtrim&#x28;MSGRUMER.GruDes&#x29; &#x2b;&#x27; &#x27; &#x2b; rtrim&#x28;MZANAART.ArtDescri&#x29; &#x2b; &#x27; &#x27; &#x2b; rtrim&#x28;MZANAART.ArtCodRep&#x29;  AS descr,   &#xd;&#xa;	MZANAART.ArtCosLor AS prezzo, &#xd;&#xa;	ArtCosLor AS prezzolistino, &#xd;&#xa;	&#x27;EUR&#x27; AS currency, &#xd;&#xa;	&#x27;images&#x2f;stories&#x2f;virtuemart&#x2f;product&#x2f;&#x27; &#x2b; ltrim&#x28;rtrim&#x28;lower&#x28;cast&#x28;MZANAART.Artnote as varchar&#x28;max&#x29;&#x29;&#x29;&#x29;&#x29; &#x2b; &#x27;.jpg&#x27; AS product_full_image, &#xd;&#xa;	&#x27;images&#x2f;stories&#x2f;virtuemart&#x2f;product&#x2f;resized&#x2f;&#x27; &#x2b; ltrim&#x28;rtrim&#x28;lower&#x28;cast&#x28;MZANAART.Artnote as varchar&#x28;max&#x29;&#x29;&#x29;&#x29;&#x29; &#x2b; &#x27;.jpg&#x27; AS product_thumb_image, &#xd;&#xa;	&#x27;Y&#x27; AS product_publish, &#xd;&#xa;	&#x27;21&#x27; AS iva, &#xd;&#xa;	CASE WHEN ARTCOSSC1 &#x3e; 0 THEN ARTCOSSC1 ELSE ARTCOSSC2 END AS product_in_stock, &#xd;&#xa;	&#x27;12234563&#x27; AS product_available, &#xd;&#xa;	&#x27;N&#x27; AS product_special, &#xd;&#xa;	MSFAM.FamCod AS codca5, &#xd;&#xa;	MSFAM.FamDes AS desca5, &#xd;&#xa;	MSTIPPRO.TprCod AS codca2, &#xd;&#xa;	MSTIPPRO.TprDes AS desca2, &#xd;&#xa;	MSSET.SetCod AS codca3, &#xd;&#xa;	MSSET.SetDes AS desca3, &#xd;&#xa;	MSREP.RepCod AS codca4, &#xd;&#xa;	MSREP.RepDes AS desca4, &#xd;&#xa;	MSSCAFFA.ScaCod AS codca1, &#xd;&#xa;	MSSCAFFA.ScaDes AS desca1, &#xd;&#xa;	MSVOCDOG.VodCod AS codca6, &#xd;&#xa;	MSVOCDOG.VodDes AS desca6,&#xd;&#xa;MZANAART.Artnote2 as classificazione,&#xd;&#xa;replace&#x28;cast&#x28;MZANAART.Artnote2 as varchar&#x28;max&#x29;&#x29; , &#x27;,&#x27;,&#x27;&#x3c;br&#x3e;&#x27;&#x29; as misurecompatibili,&#xd;&#xa;	getdate&#x28;&#x29; as news_from_Date&#xd;&#xa;   , dateadd&#x28;day, 30,getdate&#x28;&#x29;&#x29; as news_to_Date&#xd;&#xa;,substring&#x28;rtrim&#x28;ltrim&#x28;MZANAART.ArtCodfor&#x29;&#x29;,1,13&#x29; AS eancode&#xd;&#xa;, artblocco&#xd;&#xa;FROM MZANAART INNER JOIN MSGRUMER ON MZANAART.ArtCodGru &#x3d; MSGRUMER.GruCod&#xd;&#xa;	 LEFT OUTER JOIN MSCATOMO ON MZANAART.ArtCodCat &#x3d; MSCATOMO.CatMCod&#xd;&#xa;	 LEFT OUTER JOIN MSFAM ON MZANAART.ArtCodFam &#x3d; MSFAM.FamCod&#xd;&#xa;	 LEFT OUTER JOIN MSTIPPRO ON MZANAART.ArtTipPro &#x3d; MSTIPPRO.TprCod&#xd;&#xa;	 LEFT OUTER JOIN MSSET ON MZANAART.ArtCodSet &#x3d; MSSET.SetCod&#xd;&#xa;	 LEFT OUTER JOIN MSREP ON MSREP.RepCod &#x3d; MZANAART.ArtCodRep&#xd;&#xa;	 LEFT OUTER JOIN MSSCAFFA ON MZANAART.ArtCodSca &#x3d; MSSCAFFA.ScaCod&#xd;&#xa;	 LEFT OUTER JOIN MSVOCDOG ON MZANAART.ArtVocDog &#x3d; MSVOCDOG.VodCod&#xd;&#xa;WHERE &#xd;&#xa;--MZANAART.ARTCODICE like &#x27;CATWEIM3&#x25;&#x27; and &#xd;&#xa;MSCATOMO.CatMDes  like &#x27;CAT&#x25;&#x27; &#xd;&#xa;and artblocco in &#x28;&#x27;T&#x27;,&#x27;W&#x27;&#x29;</sql>
