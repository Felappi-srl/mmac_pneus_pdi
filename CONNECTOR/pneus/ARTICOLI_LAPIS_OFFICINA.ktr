    <connection>FELA</connection>
    <sql>SELECT distinct MSGRUMER.GruDes AS Marchio, &#xa;	MZANAART.ArtCodice AS Codice, &#xa;	MZANAART.ArtDescri AS Nome, &#xa;---------------------------------------------------- SHORT DESC &#xd;&#xa;rtrim&#x28;MZANAART.ArtDescri&#x29; &#x2b; &#x27; &#x27; &#x2b; rtrim&#x28;MZANAART.ArtCodRep&#x29; &#x2b; &#x27;&#x2f;&#x27; &#x2b;rtrim&#x28;MZANAART.ArtCodSet&#x29; &#xd;&#xa;&#x2b;&#xd;&#xa;case when rtrim&#x28;MSCATOMO.CatMDes&#x29; in  &#x28;&#x27;SCOOTER&#x27;,&#x27;MOTO&#x27;&#x29; then&#xd;&#xa;	case when rtrim&#x28;coalesce&#x28;MSSCAFFA.ScaCod,&#x27;&#x27;&#x29;&#x29; &#x3d; &#x27;W&#x27; then&#xd;&#xa;	&#x27; ZR&#x27; else&#xd;&#xa;	&#x27; R&#x27; end&#xd;&#xa;	else&#xd;&#xa; &#x27; R&#x27; &#xd;&#xa;end&#xd;&#xa;&#x2b;rtrim&#x28;coalesce&#x28;MZANAART.ArtCodFam,&#x27;&#x27;&#x29;&#x29; &#x2b; &#x27; &#x27; &#x2b; rtrim&#x28;coalesce&#x28;MSVOCDOG.VodDes,&#x27;&#x27;&#x29;&#x29; &#x2b; rtrim&#x28;coalesce&#x28;MSSCAFFA.ScaCod,&#x27;&#x27;&#x29;&#x29; &#x2b;&#x27; &#x27; &#x2b; rtrim&#x28;MSGRUMER.GruDes&#x29; AS short_desc_DB,&#xd;&#xa;---------------------------------------------------- SHORT DESC &#xa;	rtrim&#x28;MSGRUMER.GruDes&#x29; &#x2b;&#x27; &#x27; &#x2b; rtrim&#x28;MZANAART.ArtDescri&#x29; &#x2b; &#x27; &#x27; &#x2b; rtrim&#x28;MZANAART.ArtCodRep&#x29; &#x2b; &#x27;&#x2f;&#x27; &#x2b;rtrim&#x28;MZANAART.ArtCodSet&#x29; &#xd;&#xa;&#x2b;&#xd;&#xa;case when rtrim&#x28;MSCATOMO.CatMDes&#x29; in  &#x28;&#x27;SCOOTER&#x27;,&#x27;MOTO&#x27;&#x29; then&#xd;&#xa;	case when rtrim&#x28;coalesce&#x28;MSSCAFFA.ScaCod,&#x27;&#x27;&#x29;&#x29; &#x3d; &#x27;W&#x27; then&#xd;&#xa;	&#x27; ZR&#x27; else&#xd;&#xa;	&#x27; R&#x27; end&#xd;&#xa;	else&#xd;&#xa; &#x27; R&#x27; &#xd;&#xa;end &#x2b;rtrim&#x28;coalesce&#x28;MZANAART.ArtCodFam,&#x27;&#x27;&#x29;&#x29; &#x2b; &#x27; &#x27; &#x2b; rtrim&#x28;coalesce&#x28;MSVOCDOG.VodDes,&#x27;&#x27;&#x29;&#x29; &#x2b; rtrim&#x28;coalesce&#x28;MSSCAFFA.ScaCod,&#x27;&#x27;&#x29;&#x29;  AS descrizione_db, &#xa;	rtrim&#x28;MSCATOMO.CatMDes&#x29; AS category_path, &#xa;&#xa;	MZANAART.ArtCosNet AS prezzo, &#xa;	ArtCosLor AS prezzolistino, &#xa;	&#x28;MZMAGVAR.MVAQTAGIA - MZMAGVAR.MVAQTAIMP&#x29; AS product_in_stock&#xd;&#xa;FROM MZANAART LEFT JOIN MSGRUMER ON MZANAART.ArtCodGru &#x3d; MSGRUMER.GruCod&#xa;	 LEFT OUTER JOIN MSCATOMO ON MZANAART.ArtCodCat &#x3d; MSCATOMO.CatMCod&#xa;	 LEFT OUTER JOIN MSFAM ON MZANAART.ArtCodFam &#x3d; MSFAM.FamCod&#xa;	 LEFT OUTER JOIN MSTIPPRO ON MZANAART.ArtTipPro &#x3d; MSTIPPRO.TprCod&#xa;	 LEFT OUTER JOIN MSSET ON MZANAART.ArtCodSet &#x3d; MSSET.SetCod&#xa;	 LEFT OUTER JOIN MSREP ON MSREP.RepCod &#x3d; MZANAART.ArtCodRep&#xa;	 LEFT OUTER JOIN MSSCAFFA ON MZANAART.ArtCodSca &#x3d; MSSCAFFA.ScaCod&#xa;	 LEFT OUTER JOIN MSVOCDOG ON MZANAART.ArtVocDog &#x3d; MSVOCDOG.VodCod&#xd;&#xa;inner join MZMAGVAR on MZANAART.ArtCodice &#x3d; MZMAGVAR.ARTCODICE&#xa;where&#xd;&#xa; MZMAGVAR.MVACODMAG&#x3d;&#x27;05&#x27; AND MZMAGVAR.MVADATINV &#x3d; &#x27;1753-01-01&#x27;&#xd;&#xa;and &#x28;MZMAGVAR.MVAQTAGIA - MZMAGVAR.MVAQTAIMP&#x29; &#x3e; 0&#xd;&#xa;-- and MZANAART.ArtCodice like &#x27;17&#x25;&#x27;&#xd;&#xa;order by  MZANAART.ArtCodice </sql>
