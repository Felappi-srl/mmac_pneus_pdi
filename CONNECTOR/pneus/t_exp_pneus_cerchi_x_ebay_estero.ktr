    <sql>SELECT MSGRUMER.GruDes AS Marchio, &#xa;	MZANAART.ArtCodice AS Codice, &#xa;	MZANAART.ArtDescri AS Nome, &#xa;	 rtrim&#x28;MSGRUMER.GruDes&#x29; &#x2b;&#x27; &#x27;&#x2b; rtrim&#x28;MZANAART.ArtDescri&#x29; &#x2b; &#x27; &#x27;  &#x2b;&#xa;	CASE when MSTIPPRO.TprCod is null then&#xa;        rtrim&#x28;coalesce&#x28;MZANAART.ArtCodFam,&#x27;&#x27;&#x29;&#x29;  &#x2b; &#x27; pollici&#x27;&#xa;	else&#xa;	rtrim&#x28;coalesce&#x28;MSREP.RepDes,&#x27;&#x27;&#x29;&#x29; &#x2b; &#x27;x&#x27; &#x2b; rtrim&#x28;coalesce&#x28;MSFAM.FamCod,&#x27;&#x27;&#x29;&#x29; &#x2b; &#x27; &#x27; &#x2b; rtrim&#x28;coalesce&#x28;MSVOCDOG.VodDes,&#x27;&#x27;&#x29;&#x29; &#x2b; &#x27;x&#x27; &#x2b; rtrim&#x28;coalesce&#x28;MSSET.SetCod,&#x27;&#x27;&#x29;&#x29; &#xa;end &#xa;AS short_desc, &#xa;	rtrim&#x28;MSGRUMER.GruDes&#x29; &#x2b;&#x27; &#x27; &#x2b; rtrim&#x28;MZANAART.ArtDescri&#x29; &#x2b; &#x27; &#x27;  &#x2b;&#xa;	CASE when MSTIPPRO.TprCod is null then&#xa;        rtrim&#x28;coalesce&#x28;MZANAART.ArtCodFam,&#x27;&#x27;&#x29;&#x29;  &#x2b; &#x27; pollici&#x27;&#xa;	else&#xa;	rtrim&#x28;coalesce&#x28;MSREP.RepDes,&#x27;&#x27;&#x29;&#x29; &#x2b; &#x27;x&#x27; &#x2b; rtrim&#x28;coalesce&#x28;MSFAM.FamCod,&#x27;&#x27;&#x29;&#x29; &#x2b; &#x27; &#x27; &#x2b; rtrim&#x28;coalesce&#x28;MSVOCDOG.VodDes,&#x27;&#x27;&#x29;&#x29; &#x2b; &#x27;x&#x27; &#x2b; rtrim&#x28;coalesce&#x28;MSSET.SetCod,&#x27;&#x27;&#x29;&#x29; &#xa;end  AS descrizione, &#xa;	&#x27;CERCHI IN LEGA&#x27; AS category_path, &#xa;	&#x27;CERCHI IN LEGA&#x27;  &#x2b; &#x27;&#x7c;&#x27; &#x2b; &#x27;CERCHI IN LEGA&#x23;&#x27; &#x2b; rtrim&#x28;MSFAM.FamDes&#x29;&#x2b; &#x27; pollici&#x27; AS full_category_path, &#xa;	MZANAART.ArtCosLor AS prezzo, &#xa;	ArtCosLor AS prezzolistino, &#xa;	&#x27;EUR&#x27; AS currency, &#xa;	&#x27;http&#x3a;&#x2f;&#x2f;s3-eu-west-1.amazonaws.com&#x2f;cdn.pneus-center.it&#x2f;web&#x2f;foto&#x2f;&#x27; &#x2b; ltrim&#x28;rtrim&#x28;lower&#x28;cast&#x28;MZANAART.Artnote as varchar&#x28;max&#x29;&#x29;&#x29;&#x29;&#x29; &#x2b; &#x27;.jpg&#x27; AS product_full_image, &#xd;&#xa;	&#x27;http&#x3a;&#x2f;&#x2f;s3-eu-west-1.amazonaws.com&#x2f;cdn.pneus-center.it&#x2f;web&#x2f;foto&#x2f;resized&#x2f;&#x27; &#x2b; ltrim&#x28;rtrim&#x28;lower&#x28;cast&#x28;MZANAART.Artnote as varchar&#x28;max&#x29;&#x29;&#x29;&#x29;&#x29; &#x2b; &#x27;.jpg&#x27; AS product_thumb_image, &#xd;&#xa;	&#x27;Y&#x27; AS product_publish, &#xa;	&#x27;21&#x27; AS iva, &#xa;	CASE WHEN ARTCOSSC1 &#x3e; 0 THEN ARTCOSSC1 ELSE ARTCOSSC2 END AS product_in_stock, &#xa;	&#x27;12234563&#x27; AS product_available, &#xa;	&#x27;N&#x27; AS product_special, &#xa;	MSFAM.FamCod AS codca1, &#xa;	MSFAM.FamDes AS desca1, &#xa;	MSTIPPRO.TprCod AS codca2, &#xa;	MSTIPPRO.TprDes AS desca2, &#xa;	MSSET.SetCod AS codca3, &#xa;	MSSET.SetDes AS desca3, &#xa;	MSREP.RepCod AS codca4, &#xa;	MSREP.RepDes AS desca4, &#xa;	MSSCAFFA.ScaCod AS codca5, &#xa;	MSSCAFFA.ScaDes AS desca5, &#xa;	MSVOCDOG.VodCod AS codca6, &#xa;	MSVOCDOG.VodDes AS desca6,&#xd;&#xa;&#x27;Y&#x27; as pubblicato,&#xd;&#xa;replace&#x28;cast&#x28;MZANAART.Artnote3 as varchar&#x28;max&#x29;&#x29;,&#x27; &#x27;,&#x27;&#x27;&#x29; as altreCatEbay,&#xd;&#xa;rtrim&#x28;ltrim&#x28;MZANAART.ArtCodfor&#x29;&#x29; AS eancode&#xa;FROM MZANAART INNER JOIN MSGRUMER ON MZANAART.ArtCodGru &#x3d; MSGRUMER.GruCod&#xa;	 LEFT OUTER JOIN MSCATOMO ON MZANAART.ArtCodCat &#x3d; MSCATOMO.CatMCod&#xa;	 LEFT OUTER JOIN MSFAM ON MZANAART.ArtCodFam &#x3d; MSFAM.FamCod&#xa;	 LEFT OUTER JOIN MSTIPPRO ON MZANAART.ArtTipPro &#x3d; MSTIPPRO.TprCod&#xa;	 LEFT OUTER JOIN MSSET ON MZANAART.ArtCodSet &#x3d; MSSET.SetCod&#xa;	 LEFT OUTER JOIN MSREP ON MSREP.RepCod &#x3d; MZANAART.ArtCodRep&#xa;	 LEFT OUTER JOIN MSSCAFFA ON MZANAART.ArtCodSca &#x3d; MSSCAFFA.ScaCod&#xa;	 LEFT OUTER JOIN MSVOCDOG ON MZANAART.ArtVocDog &#x3d; MSVOCDOG.VodCod&#xa;WHERE &#xd;&#xa;cast&#x28;MZANAART.Artnote3 as varchar&#x28;max&#x29;&#x29; &#x3c;&#x3e;&#x27;&#x27; and&#xa;--MZANAART.ARTCODICE &#x3d; &#x27;AC127018055COR3&#x27; and &#xa;MSCATOMO.CatMDes  like &#x27;CERCHI&#x25;&#x27; and&#xa;--MZANAART.ArtDescri like &#x27;ACCOPPIATA&#x25;&#x27; and&#xd;&#xa;artblocco in &#x28;&#x27;T&#x27;,&#x27;E&#x27;&#x29; and ARTCODVDS &#x3e; 0</sql>
