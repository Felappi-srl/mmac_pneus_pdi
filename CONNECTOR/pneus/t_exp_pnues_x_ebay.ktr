    <connection>INPUT</connection>
    <sql>SELECT MSGRUMER.GruDes AS Marchio, &#xa;	MZANAART.ArtCodice AS Codice, &#xa;	MZANAART.ArtDescri AS Nome, &#xa;---------------------------------------------------- SHORT DESC &#xd;&#xa;rtrim&#x28;MZANAART.ArtDescri&#x29; &#x2b; &#x27; &#x27; &#x2b; rtrim&#x28;MZANAART.ArtCodRep&#x29; &#x2b; &#x27;&#x2f;&#x27; &#x2b;rtrim&#x28;MZANAART.ArtCodSet&#x29; &#xd;&#xa;&#x2b;&#xd;&#xa;case when rtrim&#x28;MSCATOMO.CatMDes&#x29; in  &#x28;&#x27;SCOOTER&#x27;,&#x27;MOTO&#x27;&#x29; then&#xd;&#xa;	case when rtrim&#x28;coalesce&#x28;MSSCAFFA.ScaCod,&#x27;&#x27;&#x29;&#x29; &#x3d; &#x27;W&#x27; then&#xd;&#xa;	&#x27; ZR&#x27; else&#xd;&#xa;	&#x27; R&#x27; end&#xd;&#xa;	else&#xd;&#xa; &#x27; R&#x27; &#xd;&#xa;end&#xd;&#xa;&#x2b;rtrim&#x28;coalesce&#x28;MZANAART.ArtCodFam,&#x27;&#x27;&#x29;&#x29; &#x2b; &#x27; &#x27; &#x2b; rtrim&#x28;coalesce&#x28;MSVOCDOG.VodDes,&#x27;&#x27;&#x29;&#x29; &#x2b; rtrim&#x28;coalesce&#x28;MSSCAFFA.ScaCod,&#x27;&#x27;&#x29;&#x29; &#x2b;&#x27; &#x27; &#x2b; rtrim&#x28;MSGRUMER.GruDes&#x29; AS short_desc_DB,&#xd;&#xa;---------------------------------------------------- SHORT DESC &#xa;	rtrim&#x28;MSGRUMER.GruDes&#x29; &#x2b;&#x27; &#x27; &#x2b; rtrim&#x28;MZANAART.ArtDescri&#x29; &#x2b; &#x27; &#x27; &#x2b; rtrim&#x28;MZANAART.ArtCodRep&#x29; &#x2b; &#x27;&#x2f;&#x27; &#x2b;rtrim&#x28;MZANAART.ArtCodSet&#x29; &#xd;&#xa;&#x2b;&#xd;&#xa;case when rtrim&#x28;MSCATOMO.CatMDes&#x29; in  &#x28;&#x27;SCOOTER&#x27;,&#x27;MOTO&#x27;&#x29; then&#xd;&#xa;	case when rtrim&#x28;coalesce&#x28;MSSCAFFA.ScaCod,&#x27;&#x27;&#x29;&#x29; &#x3d; &#x27;W&#x27; then&#xd;&#xa;	&#x27; ZR&#x27; else&#xd;&#xa;	&#x27; R&#x27; end&#xd;&#xa;	else&#xd;&#xa; &#x27; R&#x27; &#xd;&#xa;end &#x2b;rtrim&#x28;coalesce&#x28;MZANAART.ArtCodFam,&#x27;&#x27;&#x29;&#x29; &#x2b; &#x27; &#x27; &#x2b; rtrim&#x28;coalesce&#x28;MSVOCDOG.VodDes,&#x27;&#x27;&#x29;&#x29; &#x2b; rtrim&#x28;coalesce&#x28;MSSCAFFA.ScaCod,&#x27;&#x27;&#x29;&#x29;  AS descrizione_db, &#xa;	rtrim&#x28;MSCATOMO.CatMDes&#x29; AS category_path, &#xa;	rtrim&#x28;MSCATOMO.CatMDes&#x29; &#x2b; &#x27;&#x7c;&#x27; &#x2b; rtrim&#x28;MSCATOMO.CatMDes&#x29; &#x2b; &#xa;	case when len&#x28;MSSET.SetCod&#x29; &#x3e; 0 then &#x27;&#x23;Raggio &#x27;  else &#x27;&#x23;Coppia &#x27; END  &#xa;	&#x2b; rtrim&#x28;MSFAM.FamDes&#x29; &#x2b; &#x27;&#x7c;&#x27; &#x2b;&#xa;	rtrim&#x28;MSCATOMO.CatMDes&#x29; &#x2b; &#xa;	case when len&#x28;MSSET.SetCod&#x29; &#x3e; 0 then &#x27;&#x23;Raggio &#x27;  else &#x27;&#x23;Coppia &#x27; END &#xa;	&#x2b; rtrim&#x28;MSFAM.FamDes&#x29; &#x2b;&#xa;	case when len&#x28;MZANAART.ArtCodSet&#x29; &#x3e; 0 then&#xa;	  &#x27;&#x23;&#x27; &#x2b; rtrim&#x28;MZANAART.ArtCodRep&#x29; &#x2b; &#xa;	  case when len&#x28;MZANAART.ArtCodSet&#x29; &#x3e; 0 then &#x27;-&#x27; &#x2b; MZANAART.ArtCodSet ELSE &#x27;&#x27; END &#xa;	else&#xa;		&#x27;&#x27;&#xa;	end&#xa;AS full_category_path, &#xa;	MZANAART.ArtCosLor AS prezzo, &#xa;	ArtCosLor AS prezzolistino, &#xa;	&#x27;EUR&#x27; AS currency, &#xa;	&#x27;http&#x3a;&#x2f;&#x2f;s3-eu-west-1.amazonaws.com&#x2f;cdn.pneus-center.it&#x2f;web&#x2f;foto&#x2f;&#x27; &#x2b; ltrim&#x28;rtrim&#x28;lower&#x28;cast&#x28;MZANAART.Artnote as varchar&#x28;max&#x29;&#x29;&#x29;&#x29;&#x29; &#x2b; &#x27;.jpg&#x27; AS product_full_image, &#xa;	&#x27;http&#x3a;&#x2f;&#x2f;s3-eu-west-1.amazonaws.com&#x2f;cdn.pneus-center.it&#x2f;web&#x2f;foto&#x2f;resized&#x2f;&#x27; &#x2b; ltrim&#x28;rtrim&#x28;lower&#x28;cast&#x28;MZANAART.Artnote as varchar&#x28;max&#x29;&#x29;&#x29;&#x29;&#x29; &#x2b; &#x27;.jpg&#x27; AS product_thumb_image, &#xa;	&#x27;Y&#x27; AS product_publish, &#xa;	&#x27;21&#x27; AS iva, &#xa;	CASE WHEN ARTCOSSC1 &#x3e; 0 THEN ARTCOSSC1 ELSE ARTCOSSC2 END AS product_in_stock, &#xa;	&#x27;12234563&#x27; AS product_available, &#xa;	&#x27;N&#x27; AS product_special, &#xa;	MSFAM.FamCod AS codca1, &#xa;	MSFAM.FamDes AS desca1, &#xa;	replace&#x28;MSTIPPRO.TprCod,&#x27;S&#x27;,&#x27;4&#x27;&#x29; AS codca2, &#xa;	MSTIPPRO.TprDes AS desca2, &#xa;	MSSET.S