    <entry>
      <name>Compila EAN</name>
      <description/>
      <type>SQL</type>
      <sql>update L1
set l1.ean = l2.ean
from linky_integro_articoli as L1
inner join linky_integro_articoli as L2
on l1.IPCODE=l2.IPCODE and l1.PARTNER&lt;&gt;l2.PARTNER
and l1.MARCA=l2.MARCA and l1.EAN is null and l2.EAN is not null
and l1.LARGHEZZA=l2.LARGHEZZA
and l1.serie=l2.serie
and l1.CODICECERCHIO=l2.CODICECERCHIO;

UPDATE linky_integro_articoli 
SET EAN = 
CASE 
	WHEN LEN (rtrim(ltrim(EAN)) ) = 10 THEN '000' + rtrim(ltrim(EAN))
	WHEN LEN (rtrim(ltrim(EAN)) ) = 11 THEN '00' + rtrim(ltrim(EAN))
	WHEN LEN (rtrim(ltrim(EAN)) ) = 12 THEN '0' + rtrim(ltrim(EAN))
	ELSE rtrim(ltrim(EAN))
END



WHERE
MARCA = 'COOPER' and ( LEN (EAN)>10 and LEN(EAN) &lt; 13 );</sql>
