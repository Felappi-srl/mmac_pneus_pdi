    <entry>
      <name>Compila Etichetta</name>
      <description/>
      <type>SQL</type>
      <sql>update L1 set 
l1.CODICEROTOLAMENTO = l2.CODICEROTOLAMENTO
,l1.ROTOLAMENTO = l2.ROTOLAMENTO
,l1.CODICEADERENZA = l2.CODICEADERENZA
,l1.ADERENZA = l2.ADERENZA
,l1.CODICERUMOROSITA = l2.CODICERUMOROSITA
,l1.RUMOROSITA = l2.RUMOROSITA
from linky_integro_articoli as L1
inner join linky_integro_articoli as L2
on l1.IPCODE=l2.IPCODE and l1.PARTNER&lt;&gt;l2.PARTNER
and l1.MARCA=l2.MARCA 
