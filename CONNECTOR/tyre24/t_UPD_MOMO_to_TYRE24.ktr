    <connection>CONNECTOR</connection>
    <sql>select distinct
  'MOMO' as partner
, partnumber as ipcode
,	((((cast(replace(PREZZOPUBBLICO,',','.') as float))*.69))) AS prezzo
,	((((cast(replace(PREZZOPUBBLICO,',','.') as float))*.69))) AS prezzolistino
, 'CERCHI IN LEGA#' + raggio + ' pollici' AS category_path
, 'CERCHI IN LEGA|CERCHI IN LEGA#' + raggio + ' pollici' AS full_category_path
,  'MOMO' as marchio
, partnumber as codice
, upper('CERCHIO IN LEGA MOMO  ' + wheelmodel + ' ' + finishing +' ' + replace(canale,',','.')+'x' + raggio + ' ' + numerofori + 'x' + replace(interasse,',','.') + ' ET ' + offset) as nome
, upper( 'CERCHIO IN LEGA MOMO MODELLO ' + wheelmodel + ' ' + finishing +' ' + replace(canale,',','.')+'x' + raggio + ' ' + numerofori + 'x' + replace(interasse,',','.') + ' ET ' + offset + ' ' + raggio + ' POLLICI') as short_desc
, upper('CERCHIO IN LEGA MOMO MODELLO ' + wheelmodel + ' ' + finishing +' ' + replace(canale,',','.')+'x' + raggio + ' ' + numerofori + 'x' + replace(interasse,',','.')+ ' ET ' + offset + ' ' + raggio + ' POLLICI') as descrizione
, 'EUR' as currency_id
,	'http://s3-eu-west-1.amazonaws.com/cdn.pneus-center.it/web/foto/' + ltrim(rtrim(lower(cast(IMMAGINE as varchar(max))))) + '.jpg' AS product_full_image 
,	'http://s3-eu-west-1.amazonaws.com/cdn.pneus-center.it/web/foto/resized/' + ltrim(rtrim(lower(cast(IMMAGINE as varchar(max))))) + '.jpg' AS product_thumb_image 
	,'Y' AS product_publish
	,'22' AS iva, 
	coalesce(GIACENZA,0) AS product_in_stock,  
'12234563' as product_available,
	'N' AS product_special ,
	RAGGIO AS codca1, 
	RAGGIO AS desca1, 
	replace(OFFSET,',','.') AS codca2, 
	replace(OFFSET,',','.') AS desca2, 
	cast(replace(replace(interasse,',','.'),'X ','') as numeric(4,0)) AS codca3, 
	cast(replace(replace(interasse,',','.'),'X ','') as numeric(4,0)) AS desca3, 
	replace(canale,',','.') AS codca4, 
	replace(canale,',','.') AS desca4, 
	'' AS codca5, 
	'' AS desca5, 
	NUMEROFORI AS codca6, 
	NUMEROFORI AS desca6,
case when isnumeric(eancode)=1 then eancode else '' end as eancode,
'VETTURA' as settore
,replace(cb,'Ø','') as cb,finishing
from linky_articoli_momo
WHERE IMMAGINE IS NOT NULL and immagine &lt;&gt; ''  and PREZZOPUBBLICO is not null
--and WHEELMODEL like '%ACCIAIO%'
--and ipcode='000601'
--and partnumber='F8080MUQM42GD2'
and coalesce(GIACENZA,0) >0 
and canale is not null
and interasse not in ('100:108','100:114,3')
order by partnumber,eancode</sql>
