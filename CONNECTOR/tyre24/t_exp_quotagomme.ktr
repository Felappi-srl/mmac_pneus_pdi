    <sql>SELECT&#xd;&#xa;dbo.&#x5b;linky_tyre24&#x5d;.IPCODE AS &#x5b;codice_del_costruttore&#x5d;,&#xd;&#xa;dbo.&#x5b;linky_tyre24&#x5d;.EANCODE AS &#x5b;ean_13&#x5d;,&#xd;&#xa;dbo.&#x5b;linky_tyre24&#x5d;.MARCA AS &#x5b;marca_pneumatico&#x5d;,&#xd;&#xa;dbo.&#x5b;linky_tyre24&#x5d;.DESCRIZIONE AS &#x5b;modello_pneumatico&#x5d;,&#xd;&#xa;&#x27;&#x27; AS &#x5b;descrizione_pneumatico&#x5d;,&#xd;&#xa;dbo.&#x5b;linky_tyre24&#x5d;.GIACENZA AS quantit&#xe0;,&#xd;&#xa;dbo.&#x5b;linky_tyre24&#x5d;.LISTINO AS &#x5b;prezzo_pz_1&#x5d;,&#xd;&#xa;&#x27;&#x27; AS &#x5b;prezzo_pz_2&#x5d;,&#xd;&#xa;&#x27;&#x27; AS &#x5b;prezzo_pz_4&#x5d;,&#xd;&#xa;&#x27;&#x27; AS &#x5b;sconto&#x5d;,&#xd;&#xa;22 AS aliquota,&#xd;&#xa;case coalesce&#x28;codicestagione,&#x27;E&#x27;&#x29; &#xd;&#xa;	when &#x27;E&#x27; then &#x27;E&#x27;&#xd;&#xa;	when &#x27;N&#x27; then &#x27;I&#x27;&#xd;&#xa;	when &#x27;S&#x27; then &#x27;Q&#x27;&#xd;&#xa;	when &#x27;X&#x27; then &#x27;I&#x27;&#xd;&#xa;	when &#x27;PNE&#x27; then &#x27;E&#x27;&#xd;&#xa;	when &#x27;R&#x27; then &#x27;E&#x27;&#xd;&#xa;	else &#x27;Q&#x27; end as stagione&#xd;&#xa;, case coalesce&#x28;rtrim&#x28;settore&#x29;,&#x27;VETTURA&#x27;&#x29;&#xd;&#xa;	when &#x27;VETTURA&#x27; then &#x27;V&#x27;&#xd;&#xa;	when &#x27;FUORISTRADA&#x27; then &#x27;O&#x27;&#xd;&#xa;	when &#x27;ATV - GARDEN&#x27; then &#x27;Q&#x27;&#xd;&#xa;	when &#x27;VETTURA&#x27; then &#x27;V&#x27;&#xd;&#xa;	when &#x27;FUORI STRADA&#x27; then &#x27;O&#x27;&#xd;&#xa;	when &#x27;FUORISTRADAFUORISTRADAFUORISTRADA&#x27; then &#x27;O&#x27;&#xd;&#xa;	when &#x27;FUORISTRADAFUORISTRADA&#x27; then &#x27;O&#x27;&#xd;&#xa;	when &#x27;RUN FLAT&#x27; then &#x27;V&#x27;&#xd;&#xa;	when &#x27;AUTOCARRO 17,5 19,5&#x27; then &#x27;P&#x27;&#xd;&#xa;	when &#x27;AUTOCARRO&#x27; then &#x27;P&#x27;&#xd;&#xa;	when &#x27;TRASPORTO LEGGERO&#x27; then &#x27;T&#x27;&#xd;&#xa;	when &#x27;SCOOTER&#x27; then &#x27;M&#x27;&#xd;&#xa;	when &#x27;MOTO&#x27; then &#x27;M&#x27;&#xd;&#xa;else &#x27;V&#x27; end as linea_prodotto&#xd;&#xa;,larghezza as larghezza_pneumatico&#xd;&#xa;, codiceserie as rapporto_altezza&#xd;&#xa;, cerchio as diametro_cerchio&#xd;&#xa;, case when settore &#x3d;&#x27;RUN FLAT&#x27; then &#x27;1&#x27; else 0 end as run_flat&#xd;&#xa;,&#x27;&#x27; AS &#x5b;fine_serie&#x5d;,&#xd;&#xa;&#x27;&#x27; AS &#x5b;rinforzato&#x5d;,&#xd;&#xa;&#x27;&#x27; AS &#x5b;difettoso&#x5d;,&#xd;&#xa;&#x27;&#x27; AS &#x5b;bordo&#x5d;&#xd;&#xa;,codicevel as indice_di_velocita&#x300;&#xd;&#xa;, icarico as indice_di_carico&#xd;&#xa;, codicerotolamento as efficienza_energetica_pneumatico&#xd;&#xa;,codiceaderenza as aderenza_sul_bagnato&#xd;&#xa;,case when charindex&#x28;&#x27; &#x27;,coalesce&#x28;RUMOROSITA,&#x27;&#x27;&#x29; &#x29;&#x3e;-1 then&#xd;&#xa;	case  ltrim&#x28;rtrim&#x28;substring&#x28;RUMOROSITA, charindex&#x28;&#x27; &#x27;,RUMOROSITA&#x29;&#x2b;1 ,10&#x29;&#x29;&#x29;&#xd;&#xa;when &#x27;&#x29;&#x27; then &#x27;1&#x27;&#xd;&#xa;when &#x27;&#x29;&#x29;&#x27; then &#x27;2&#x27;&#xd;&#xa;when &#x27;&#x29;&#x29;&#x29;&#x27; then &#x27;3&#x27;&#xd;&#xa;else &#x27;&#x27; end&#xd;&#xa;ELSE &#x27;&#x27; end&#xd;&#xa;as rumorosita_al_rotolamento&#xd;&#xa;,&#x27;&#x27; as dot&#xd;&#xa;,&#x27;&#x27; as altre_sigle&#xd;&#xa;,&#x27;&#x27; as url_immagine&#xd;&#xa;, case  rtrim&#x28;settore&#x29;  &#xd;&#xa;	when &#x27;MOTO&#x27; then &#x27;1,10&#x27;&#xd;&#xa;	when &#x27;SCOOTER&#x27; then &#x27;1,10&#x27;&#xd;&#xa;	else &#x27;2,20&#x27;&#xd;&#xa;end as costo_pfu&#xd;&#xa;FROM&#xd;&#xa;dbo.&#x5b;linky_tyre24&#x5d;&#xd;&#xa;where partner not in &#x28;&#x27;MAK&#x27;,&#x27;OZR&#x27;,&#x27;ALC&#x27;,&#x27;MOMO&#x27;, &#x27;PNS&#x27;&#x29;&#xd;&#xa;and settore &#x3c;&#x3e; &#x27;CATENE DA NEVE&#x27;&#xd;&#xa;and GIACENZA&#x3e;0&#xd;&#xa;and &#x5b;linky_tyre24&#x5d;.DESCRIZIONE is not null&#xd;&#xa;order by ipcode&#xd;&#xa;</sql>
