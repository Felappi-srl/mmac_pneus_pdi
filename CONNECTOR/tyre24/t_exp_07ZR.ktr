    <connection>LINKY</connection>
    <sql>SELECT 
EANCODE  as EAN
,IPCODE as codice_PRODUTTORE
,IPCODE as Codice_interno_distributore
,GIACENZA as Stock
,PREZZOIT as Prezzo_di_vendita
,MARCA as Marca

, REPLACE(
SUBSTRING(DESCRIZIONE, (CHARINDEX('MODELLO ', DESCRIZIONE)+ LEN('MODELLO ')), (CHARINDEX( ' ' + LARGHEZZA+'X', DESCRIZIONE)) - ( (CHARINDEX('MODELLO ', DESCRIZIONE)+ LEN('MODELLO ')) )   ) ,
 ' ' + FINISHING, '') as Modello

,'' as Stagione
,DESCRIZIONE as Descrizione_completa
,CASE WHEN DESCRIZIONE LIKE 'CERCHIO IN LEGA%' THEN 'LEGA' ELSE 'ACCIAIO' END as Tipo
,CERCHIO as Diametro
,LARGHEZZA as Larghezza_massima_cerchio
,SUBSTRING(DESCRIZIONE, CHARINDEX('ET ', DESCRIZIONE) + 3, 2) as ET
, SUBSTRING(DESCRIZIONE, CHARINDEX(('X'+CERCHIO), DESCRIZIONE) + (LEN(CERCHIO)+2), 10) as bloc_FORI
,SUBSTRING(SUBSTRING(DESCRIZIONE, CHARINDEX(('X'+CERCHIO), DESCRIZIONE) + (LEN(CERCHIO)+2), 10), 1, 1) as Numero_di_fori
,'' as Numero_di_fori_2

, SUBSTRING(
SUBSTRING(DESCRIZIONE, CHARINDEX(('X'+CERCHIO), DESCRIZIONE) + (LEN(CERCHIO)+2), 10), 
(CHARINDEX('X', SUBSTRING(DESCRIZIONE, CHARINDEX(('X'+CERCHIO), DESCRIZIONE) + (LEN(CERCHIO)+2), 10)) +1),
(CHARINDEX(' ', SUBSTRING(DESCRIZIONE, CHARINDEX(('X'+CERCHIO), DESCRIZIONE) + (LEN(CERCHIO)+2), 10)) -3)
) as PCD

,'' as PCD_2
,CB as Foro_mozzo
,FINISHING as Finiture
,'' as Certificazione_NAD_ECE_
,'' as prefisso
,'' as Indice_di_carico

FROM linky_tyre24 where giacenza >3 and listino >15
and partner in ('MAK','MOMO','ALC','TRW', 'PNS')
and listino is not null</sql>
