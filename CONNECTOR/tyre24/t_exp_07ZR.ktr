, COALESCE (SKU, IPCODE) as CODICE_DISTRIBUTORE
, GIACENZA as STOCK
, PREZZOIT as PREZZO_DI_VENDITA
, MARCA as MARCA
, SETTORE as CATEGORIA
, STAGIONE as Stagione
, DESCRIZIONE as Descrizione_completa
, CODICEROTOLAMENTO as Fuel	
, CODICEADERENZA as Wetgrip
, RUMOROSITA as Noise
, DOT as DOT
, CASE WHEN RUNFLAT = 'Y' THEN 'RUNFLAT' ELSE '' END as RFT
, partner



FROM linky_tyre24 

where giacenza >3 and listino >15
-- and partner NOT IN ('MAK','MOMO','ALC','TRW', 'PNS', 'OZR')
and partner in (
Select codicepartner from linky_partners where tipo='PNEUMATICI' 
-- and codicepartner not in ('PN','CST')
)
and listino is not null 
and MARCA &lt;&gt; 'INTERSTATE'
-- and descrizione LIKE '%DOT%'
-- and ipcode = 'TL00212100 '
ORDER BY MARCA</sql>
