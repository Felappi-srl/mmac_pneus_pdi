      <yloc>144</yloc>
    </entry>
    <entry>
      <name>PREZZO &#x3c;&#x3e;1 IT e DE FR</name>
      <description/>
      <type>SQL</type>
      <sql>update t24&#xa;set t24.PREZZOIT &#x3d; &#xa;case when &#x28;t24.PREZZORIFERIMENTO &#x2b; RICARICO&#x29; &#x3e; &#x28;comp.BESTPRICE-GAP&#x29; then t24.PREZZORIFERIMENTO &#x2b; RICARICO else comp.BESTPRICE-GAP end&#xa;from linky_tyre24 as t24&#xa;inner join LINKY_T24_COMPPRICE as comp on t24.IPCODE &#x3d; comp.IPCODE and comp.siteid&#x3d;&#x27;IT&#x27;&#xa;, linky_tyre24_params&#xa;where comp.POSIZIONE&#x3c;&#x3e;&#x27;1.&#x27; and t24.partner&#x3c;&#x3e;&#x27;PN&#x27;&#xa;--&#xa;update t24&#xa;set t24.PREZZODE &#x3d; &#xa;case when &#x28;t24.PREZZORIFERIMENTO &#x2b; RICARICO&#x2b;10&#x29; &#x3e; &#x28;comp.BESTPRICE-GAP&#x29; then t24.PREZZORIFERIMENTO &#x2b; RICARICO &#x2b;10 else comp.BESTPRICE-GAP end&#xa;from linky_tyre24 as t24&#xa;inner join LINKY_T24_COMPPRICE as comp on t24.IPCODE &#x3d; comp.IPCODE and comp.siteid&#x3d;&#x27;DE&#x27;&#xa;, linky_tyre24_params&#xa;where comp.POSIZIONE&#x3c;&#x3e;&#x27;1.&#x27; and t24.partner&#x3c;&#x3e;&#x27;PN&#x27; and t24.partner not in &#x28;select codicepartner from linky_partners where tipo&#x3d;&#x27;CERCHI&#x27;&#x29;&#x3b;&#xa;--&#xa;update t24&#xa;set t24.PREZZOFR &#x3d; &#xa;case when &#x28;t24.PREZZORIFERIMENTO &#x2b; RICARICO&#x2b;10&#x29; &#x3e; &#x28;comp.BESTPRICE-GAP&#x29; then t24.PREZZORIFERIMENTO &#x2b; RICARICO &#x2b;10 else comp.BESTPRICE-GAP end&#xa;from linky_tyre24 as t24&#xa;inner join LINKY_T24_COMPPRICE as comp on t24.IPCODE &#x3d; comp.IPCODE and comp.siteid&#x3d;&#x27;FR&#x27;&#xa;, linky_tyre24_params&#xa;where comp.POSIZIONE&#x3c;&#x3e;&#x27;1.&#x27; and t24.partner&#x3c;&#x3e;&#x27;PN&#x27; and t24.partner not in &#x28;select codicepartner from linky_partners where tipo&#x3d;&#x27;CERCHI&#x27;&#x29;&#x3b;</sql>
