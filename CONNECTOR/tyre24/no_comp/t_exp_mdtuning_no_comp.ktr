    <sql>SELECT
  ipcode as ipcodeperunivoco,
  CODICETYRE as ipcode
--, partner
, DESCRIZIONE
, MARCA
, SETTORE
, CODICELARGHEZZA
, LARGHEZZA
, CODICESERIE
, SERIE
, CODICECERCHIO
, CERCHIO
, CODICEVEL
, VEL
, CODICESTAGIONE
, STAGIONE
, LISTINO
, GIACENZA
, ICARICO
, CODICEROTOLAMENTO
, ROTOLAMENTO
, CODICEADERENZA
, ADERENZA
, CODICERUMOROSITA
, case when partner in ('MAK','MOMO','OZR','ALC','TRW') then 'ALU' else 
RUMOROSITA end RUMOROSITA
,  EANCODE
, LISTINO +case when partner in ('MAK','MOMO','ALC','TRW') then 5 else 10 end as LISTINO_BELGIO
, case when partner='MAK' then PREZZOFR else
	LISTINO +case when partner in ('MOMO','ALC','TRW') then 5 else 10 end end as LISTINO_FRANCIA
, LISTINO +case when partner in ('MAK','MOMO','ALC','TRW') then 5 else 10 end as LISTINO_OLANDA
, 
case when partner='MAK' then PREZZODE else
case when marca='OZ Racing' then 0 else
LISTINO +case when partner in ('MOMO','ALC','TRW') then 5 else 10 end end end as LISTINO_GERMANIA
FROM linky_tyre24 where giacenza >3 and listino >15
and partner in ('MAK','MOMO','OZR','ALC','TRW')
and not (marca ='BRIDGESTONE' and SETTORE ='MOTO') and listino is not null 
union all
SELECT
  ipcode as ipcodeperunivoco,
  CODICETYRE as ipcode
--, partner
, DESCRIZIONE
, MARCA
, SETTORE
, CODICELARGHEZZA
, LARGHEZZA
, CODICESERIE
, SERIE
, CODICECERCHIO
, CERCHIO
, CODICEVEL
, VEL
, CODICESTAGIONE
, STAGIONE
, LISTINO
, GIACENZA
, ICARICO
, CODICEROTOLAMENTO
, ROTOLAMENTO
, CODICEADERENZA
, ADERENZA
, CODICERUMOROSITA
, case when partner in ('MAK','MOMO','OZR','ALC') then 'ALU' else 
RUMOROSITA end RUMOROSITA
, EANCODE
, LISTINO +case when partner in ('MAK','MOMO','ALC') then 13 else 10 end as LISTINO_BELGIO
, PREZZOFR as LISTINO_FRANCIA
, LISTINO +case when partner in ('MAK','MOMO','ALC') then 13 else 10 end as LISTINO_OLANDA
, 
case when marca='MSW' then 0 else
PREZZODE end as LISTINO_GERMANIA
FROM linky_tyre24 where giacenza >0 and listino >15
and partner in ('CG','DEL','YOKO','TG','TY','UNI','GO','BER','ETT','FIN')
and not (marca ='BRIDGESTONE' and SETTORE ='MOTO') and listino is not null 
union all
SELECT
  ipcode as ipcodeperunivoco,
  CODICETYRE as ipcode
--, partner
, DESCRIZIONE
, MARCA
, SETTORE
, CODICELARGHEZZA
, LARGHEZZA
, CODICESERIE
, SERIE
, CODICECERCHIO
, CERCHIO
, CODICEVEL
, VEL
, CODICESTAGIONE
, STAGIONE
, LISTINO
, GIACENZA
, ICARICO
, CODICEROTOLAMENTO
, ROTOLAMENTO
, CODICEADERENZA
, ADERENZA
, CODICERUMOROSITA
, RUMOROSITA
,  EANCODE
, LISTINO +10 as LISTINO_BELGIO
, LISTINO +10 as LISTINO_FRANCIA
, LISTINO +10 as LISTINO_OLANDA
, LISTINO +10 as LISTINO_GERMANIA
FROM linky_tyre24 where giacenza >0  and listino >15
--and partner in ('DEL','CG','TG')
and partner in ('PN')
--and ipcode='71517'
and eancode is not null and listino is not null

</sql>
