    <sql>select 
SKU
, partner
, ipcodESTD as IPCODE
, round(((prezzoscontato + 7) * 1.10 ),0) as prezzo
, round(((prezzoscontato + 7) * 1.10 ),0) as prezzoLISTINO
, REPLACE(REPLACE(SETTORE,' FUORI LISTINO',''),'4X4','FUORI STRADA')
 + '#Raggio ' + REPLACE(REPLACE(CERCHIO,'DIAMETRO ',''),'"','')
 + '#' + LARGHEZZA + '-' + SERIE
 AS Category_Path
, REPLACE(REPLACE(SETTORE,' FUORI LISTINO',''),'4X4','FUORI STRADA') + '|'
  + REPLACE(REPLACE(SETTORE,' FUORI LISTINO',''),'4X4','FUORI STRADA')
  + '#Raggio ' + REPLACE(REPLACE(CERCHIO,'DIAMETRO ',''),'"','') + '|'
  + REPLACE(REPLACE(SETTORE,' FUORI LISTINO',''),'4X4','FUORI STRADA')
 + '#Raggio ' + REPLACE(REPLACE(CERCHIO,'DIAMETRO ',''),'"','')
 + '#' + LARGHEZZA + '-' + SERIE as Full_Category_path
, marca
, ipcode as codice
, 'GOMME PNEUMATICI ' + COALESCE(TITOLOALTERNATIVO,DESCRIZIONE,'') AS NOME
, MARCA + ' GOMME PNEUMATICI ' + COALESCE(TITOLOALTERNATIVO,descrizione,'') + ' ' 
  + LARGHEZZA + '/' + coalesce(SERIE,'') + ' R' + CODICECERCHIO + ' ' + ltrim(coalesce(replace(icarico,',0',''),'')) + replace(rtrim(substring(VEL,1,2)),'Z','W')  AS SHORT_DESC
, MARCA + ' GOMME PNEUMATICI ' + COALESCE(TITOLOALTERNATIVO,'') + ' ' 
  + LARGHEZZA + '/' + coalesce(SERIE,'')  + ' R' + CODICECERCHIO + ' '  + ltrim(coalesce(replace(icarico,',0',''),'')) + replace(rtrim(substring(VEL,1,2)),'Z','W')  AS DESCRIZIONE_DB
, 'EUR' as currency
, 'images/stories/virtuemart/product/' + immagine + '.jpg' as fullimg
, 'images/stories/virtuemart/product/resized/' + immagine + '.jpg' as resimg
, case when giacenza &lt;&gt;0 then 'Y' else 'N' end as Publish
, 21 as iva
,  giacenza  as product_in_stock
, 12234563 product_available
, 'N' as product_special
, CODICECERCHIO AS CODCA1
, CODICECERCHIO AS DESCA1
, CASE WHEN CODICESTAGIONE='I' THEN 'N' ELSE 'E' END AS CODCA2
, CASE WHEN CODICESTAGIONE='I' THEN 'INVERNALI' ELSE 'ESTIVE' END AS DESCA2
, SERIE AS CODCA3
, 'SERIE ' + SERIE AS DESCA3
, LARGHEZZA AS CODCA4
, LARGHEZZA AS DESCA4
, replace(SUBSTRING(VEL,1,1),'Z','W') AS CODCA5_DB
, replace(SUBSTRING(VEL,1,1),'Z','W') +': ' +SUBSTRING(VEL,2,4) + ' Km/h' AS tmp_DESCA5
, ltrim(coalesce(replace(icarico,',0',''),'')) AS CODCA6
, ltrim(coalesce(replace(icarico,',0',''),'')) AS DESCA6,
CODICEROTOLAMENTO,
ROTOLAMENTO,
CODICEADERENZA	,
ADERENZA	,
CODICERUMOROSITA,
RUMOROSITA,
REPLACE(REPLACE(SETTORE,' FUORI LISTINO',''),'4X4','FUORI STRADA') AS SETTORE,
--case when partner='TG' then '' else 
substring(EAN,1,13) 
--end 
as ean
,prezzoscontato as prezzoriferimento
, runflat
, radiale
, TITOLOALTERNATIVO
, LARGHEZZA
, SERIE
, CODICECERCHIO as DIAMETRO
, icarico as z_icarico
from linky_integro_articoli
WHERE IMMAGINE IS NOT NULL and immagine &lt;&gt; '' 
--and partner not in ('TG')
and giacenza>0 and prezzoscontato>0
and settore in ('MOTO','SCOOTER')
--and ipcode='1208417178'
order by sku, prezzoscontato desc</sql>
