    <connection>LINKY</connection>
    <sql>SELECT distinct 
linky_integro_articoli.IPCODESTD as SKU,
linky_integro_articoli.MARCA AS BRAND,
coalesce(linky_integro_articoli.TITOLOALTERNATIVO, DESCRIZIONE) AS TITOLO,
replace(replace(linky_integro_articoli.CODICECERCHIO,',','.'),'VUOTA','') AS RAGGIO,
replace(linky_integro_articoli.SERIE,',5','.50') AS SPALLA,
linky_integro_articoli.LARGHEZZA,
linky_integro_articoli.ICARICO,
linky_integro_articoli.VEL,
linky_integro_articoli.SETTORE,
replace(replace(replace(replace( RTRIM(LTRIM(linky_integro_articoli.CODICEROTOLAMENTO)),'U',''),'N',''),'S',''), '_', '') as CODICEROTOLAMENTO,

---- linky_integro_articoli.CODICEADERENZA,
replace(replace(replace(replace( RTRIM(LTRIM(linky_integro_articoli.CODICEROTOLAMENTO)),'U',''),'N',''),'S',''), '_', '') as CODICEADERENZA,

--linky_integro_articoli.CODICERUMOROSITA
-- correzione Diego alcuni Partner hanno invertiti il codice di rumorosità
CASE
WHEN SUBSTRING (linky_integro_articoli.CODICERUMOROSITA,2,1) = '_' 
THEN SUBSTRING (linky_integro_articoli.CODICERUMOROSITA,3,2) + '_' + SUBSTRING (linky_integro_articoli.CODICERUMOROSITA,1,1)
ELSE 
linky_integro_articoli.CODICERUMOROSITA
END AS CODICERUMOROSITA,

linky_integro_articoli.EAN,
--concat(linky_integro_articoli.IMMAGINE,'.jpg') as IMMAGINE
linky_integro_articoli.IMMAGINE + '.jpg' as IMMAGINE
,'NONE' as CLASSE
, STAGIONE
, PESO
, VOLUME
, case when partner='DEL' then '48-72' else '24-48' end as DELIVERYTIME
, RUNFLAT as RUNFLAT_EXP
-- , PARTNER
FROM
linky_integro_articoli
INNER JOIN linky_magento_report ON linky_integro_articoli.IPCODESTD = linky_magento_report.IPCODE
where 
 1=1
and linky_integro_articoli.GIACENZA > 0
and linky_integro_articoli.SETTORE is not null and linky_integro_articoli.SETTORE &lt;&gt;''
and linky_integro_articoli.IMMAGINE is not null and linky_integro_articoli.IMMAGINE&lt;&gt;''
order by SKU</sql>
