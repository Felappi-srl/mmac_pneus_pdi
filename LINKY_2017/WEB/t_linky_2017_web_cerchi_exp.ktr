    <sql>SELECT DISTINCT
linky_articoli_mak.PARTNUMBER as SKU,
linky_articoli_mak.WHEELMODEL as MODELLO,
'MAK' as MARCA,
linky_articoli_mak.FINISHING,
linky_articoli_mak.RAGGIO,
replace(replace(replace(replace(canale,',0',''),',','.'),'.0',''),'J','') as CANALE,
linky_articoli_mak.NUMEROFORI,
replace(replace(replace(replace(offset,',0',''),',','.'),'.50','.5'),'J','') as OFFSET,
replace(replace(replace(replace(replace(interasse,',0',''),',','.'),'.50','.5'),'.30','.3'),'.70','.7') as INTERASSE,
replace(replace(replace(cb,'C.R. ',''),'Ø',''),',','.') as cb,
linky_articoli_mak.EANCODE,
concat(linky_articoli_mak.IMMAGINE,'.jpg') as IMMAGINE
, case when linky_articoli_mak.WHEELMODEL = 'ACCIAIO' then 'ACCIAIO' else 'LEGA' end as TIPOLOGIA
FROM linky_articoli_mak ${inner_mak}
WHERE
${where}
replace(linky_articoli_mak.GIACENZA,',','.')>0 and replace(linky_articoli_mak.PREZZO,',','.')>'0' and linky_articoli_mak.OUT_PROD=0
--and linky_articoli_mak.PARTNUMBER='F7060FFSI40FF'
union all 
SELECT DISTINCT
PARTNUMBER as SKU,
WHEELMODEL as MODELLO,
'MOMO' as MARCA,
FINISHING,
RAGGIO,
replace(replace(replace(replace(canale,',0',''),',','.'),'.0',''),'J','') as CANALE,
NUMEROFORI,
replace(replace(replace(replace(offset,',0',''),',','.'),'.50','.5'),'J','') as OFFSET,
replace(replace(replace(replace(replace(interasse,',0',''),',','.'),'.50','.5'),'.30','.3'),'.70','.7') as INTERASSE,
replace(replace(replace(cb,'C.R. ',''),'Ø',''),',','.') as cb,
EANCODE,
concat(IMMAGINE,'.jpg') as IMMAGINE
, case when WHEELMODEL = 'ACCIAIO' then 'ACCIAIO' else 'LEGA' end as TIPOLOGIA
FROM
linky_articoli_momo ${inner_momo}
WHERE
${where}
replace(linky_articoli_momo.GIACENZA,',','.')>0 and replace(linky_articoli_momo.PREZZO,',','.')>'0' 
union all 
SELECT DISTINCT
PARTNUMBER as SKU,
WHEELMODEL as MODELLO,
brand as MARCA,
FINISHING,
RAGGIO,
replace(replace(replace(replace(canale,',0',''),',','.'),'.0',''),'J','') as CANALE,
NUMEROFORI,
replace(replace(replace(replace(offset,',0',''),',','.'),'.50','.5'),'J','') as OFFSET,
replace(replace(replace(replace(replace(interasse,',0',''),',','.'),'.50','.5'),'.30','.3'),'.70','.7') as INTERASSE,
replace(replace(replace(cb,'C.R. ',''),'Ø',''),',','.') as cb,
EANCODE,
concat(IMMAGINE,'.jpg') as IMMAGINE
, case when WHEELMODEL = 'ACCIAIO' then 'ACCIAIO' else 'LEGA' end as TIPOLOGIA
FROM
linky_articoli_ozr ${inner_ozr}
WHERE
${where}
replace(replace(linky_articoli_ozr.GIACENZA,'> 12','20'),',','.')>0 and replace(linky_articoli_ozr.PREZZO,',','.')>'0'  and OUT_PROD=0
union all
SELECT DISTINCT
PARTNUMBER as SKU,
upper(WHEELMODEL) as MODELLO,
brand as MARCA,
upper(FINISHING) as finishing,
RAGGIO,
replace(replace(replace(replace(canale,',0',''),',','.'),'.0',''),'J','') as CANALE,
NUMEROFORI,
replace(replace(replace(replace(offset,',0',''),',','.'),'.50','.5'),'J','') as OFFSET,
replace(replace(replace(replace(replace(interasse,',0',''),',','.'),'.50','.5'),'.30','.3'),'.70','.7') as INTERASSE,
replace(replace(replace(cb,'C.R. ',''),'Ø',''),',','.') as cb,
EANCODE,
concat(IMMAGINE,'.jpg') as IMMAGINE
, case when WHEELMODEL = 'ACCIAIO' then 'ACCIAIO' else 'LEGA' end as TIPOLOGIA
FROM
linky_articoli_ALCAR ${inner_alcar}
WHERE
${where}
replace(replace(linky_articoli_ALCAR.GIACENZA,'> 12','20'),',','.')>0 and replace(linky_articoli_ALCAR.PREZZO,',','.')>'0' 

</sql>
