    <connection>CONNECTOR_2017</connection>
    <sql>SELECT distinct
 e.codice as product_sku
, substring((replace(var.nome, 'CERCHI IN LEGA ', 'JANTES ROUES ')),1,80) as Titolo
, replace(var.nome, 'CERCHI IN LEGA ', 'JANTES ROUES ') as titolo_header
, ebaycat
,'France' as siteid
,'FR' as country
,'FixedPriceItem' as Type
,  Marca 
, linky_calc_endprice_2016(partner,'FR','CERCHI','',prezzopubblico,0)  as product_price
, product_in_stock as product_in_stock
, case when codca1 is not null then codca1 else '' end as diametro
, case when codca3 is not null then codca3 else '' end as spalla
, case when codca4 is not null then desca4 else '' end as larghezza
, case when codca5 is not null then desca5 else '' end as ivel
, case when codca6 is not null then desca6 else '' end as icarico
, case when codca2 is not null then DESca2 else '' end as offset
, 'CERCHI' as category_path
,product_full_image
, '' as tipoutilizzo
, partner,
case when urlcertificato is not null then
'&lt;p>&lt;a href="' + urlcertificato + '" target="_blank">Download le certificat&lt;/a>&lt;/p>'
else
''
end as urlcertificato,eancode,
var.codice as mpn
, auto
FROM ebay_cerchi e inner join ebay_cerchi_app var on e.codice=var.codice
where
 var.isece=1 
and product_in_stock > 3 
and prezzolistino > 10 
and var.siteid='FR'  
and (out_prod&lt;&gt;'1' OR out_prod IS NULL)
</sql>
