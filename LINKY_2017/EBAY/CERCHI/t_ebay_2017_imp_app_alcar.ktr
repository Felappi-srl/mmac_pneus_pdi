    <sql>SELECT&#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR&#x5d;.BRAND,&#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR&#x5d;.PARTNUMBER,&#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR&#x5d;.PARTNUMBER &#x2b; &#xd;&#xa;cast&#x28;dbo.&#x5b;linky_articoli_ALCAR_auto&#x5d;.CODICETIPO as varchar&#x28;10&#x29;&#x29; as codicevar,&#xd;&#xa;upper&#x28;&#x27;CERCHI IN LEGA &#x27; &#x2b;  dbo.&#x5b;linky_articoli_ALCAR&#x5d;.BRAND &#x2b; &#x27; &#x27; &#x2b; dbo.&#x5b;linky_articoli_ALCAR&#x5d;.WHEELMODEL &#x2b; &#x27; &#x27; &#x2b; &#xd;&#xa;replace&#x28;dbo.&#x5b;linky_articoli_ALCAR&#x5d;.CANALE,&#x27;.00&#x27;,&#x27;&#x27;&#x29; &#x2b; &#x27;x&#x27; &#x2b; &#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR&#x5d;.RAGGIO &#x2b; &#x27; &#x27; &#x2b; &#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR&#x5d;.NUMEROFORI &#x2b; &#x27;x&#x27; &#x2b; &#xd;&#xa;replace&#x28;dbo.&#x5b;linky_articoli_ALCAR&#x5d;.INTERASSE,&#x27;.00&#x27;,&#x27;&#x27;&#x29; &#x2b; &#x27; ET&#x27;&#x2b; &#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR&#x5d;.OFFSET &#x2b;&#x27; &#x27; &#x2b;&#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR&#x5d;.FINISHING &#x2b; &#x27; &#x27; &#x2b;&#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR_auto&#x5d;.MARCA &#x2b; &#x27; &#x27; &#x2b; &#xd;&#xa;substring&#x28;dbo.&#x5b;linky_articoli_ALCAR_auto&#x5d;.TIPO ,charindex&#x28;&#x27; - &#x27;,dbo.&#x5b;linky_articoli_ALCAR_auto&#x5d;.TIPO&#x29; &#x2b; 3,len&#x28;dbo.&#x5b;linky_articoli_ALCAR_auto&#x5d;.TIPO&#x29;&#x29;&#xd;&#xa;&#x29; as NOME,&#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR&#x5d;.CATEGORIA,&#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR_autocerchi&#x5d;.isECE,&#xd;&#xa;&#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR_autocerchi&#x5d;.URLCERTIFICATO,&#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR_autocerchi&#x5d;.URLIMGAUTO,&#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR&#x5d;.IMMAGINE,&#xd;&#xa;dbo.&#x5b;linky_articoli_ALCAR_auto&#x5d;.MARCA as marcaauto,&#xd;&#xa;substring&#x28;dbo.&#x5b;linky_articoli_ALCAR_auto&#x5d;.TIPO ,charindex&#x28;&#x27; - &#x27;,dbo.&#x5b;linky_articoli_ALCAR_auto&#x5d;.TI