    <connection>CONNECTOR_2017</connection>
    <sql>SELECT DISTINCT CAST(rep.IDOGGETTO as CHAR) as IDOGGETTO, rep.SKU
FROM linky_ebay_report as rep
INNER JOIN ebay_cerchi_app as app ON app.codicevar = rep.SKU
INNER JOIN ebay_cerchi as ce ON ce.codice = app.codice
WHERE ce.partner = 'ALCAR' and rep.SKU = 'TREK8KA43E4059'
ORDER BY rep.SKU ASC</sql>
