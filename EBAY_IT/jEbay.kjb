    <entry>
      <name>CG Update stock</name>
      <description />
      <type>SQL</type>
      <sql>UPDATE  L
SET 
l.prezzo=prezzoscontato, 
l.prezzolistino=prezzoscontato , 
l.category_path=settore , 
l.product_publish='Y',
l.product_in_stock=	giacenza , 
l.product_available='12234563' , 
l.pubblicato=	'Y',
l.product_full_image= 'http://s3-eu-west-1.amazonaws.com/cdn.pneus-center.it/web/foto/' + ltrim(rtrim(lower(cast(immagine as varchar(max))))) + '.jpg' ,
l.product_thumb_image= 'http://s3-eu-west-1.amazonaws.com/cdn.pneus-center.it/web/foto/resized/' + ltrim(rtrim(lower(cast(immagine as varchar(max))))) + '.jpg'
,l.nome= 'GOMME PNEUMATICI ' + COALESCE(TITOLOALTERNATIVO,linky_integro_articoli.DESCRIZIONE,'')
,l.short_desc= ' GOMME PNEUMATICI ' + COALESCE(TITOLOALTERNATIVO,'') + ' ' 
  + LARGHEZZA + '/' + SERIE +
case when rtrim(SETTORE) in  ('SCOOTER','MOTO') then
	case when rtrim(coalesce(VEL,'')) = 'W' then
	' ZR' else
	' R' end
	else
 ' R' 
end + CODICECERCHIO + ' ' + ltrim(coalesce(replace(icarico,',0',''),'')) + replace(rtrim(substring(VEL,1,2)),'Z','W') + ' ' + linky_integro_articoli.marca
,l.descrizione= linky_integro_articoli.marca + ' GOMME PNEUMATICI ' + COALESCE(TITOLOALTERNATIVO,'') + ' ' 
  + LARGHEZZA + '/' + SERIE +
case when rtrim(SETTORE) in  ('SCOOTER','MOTO') then
	case when rtrim(coalesce(VEL,'')) = 'W' then
	' ZR' else
	' R' end
	else
 ' R' 
end + CODICECERCHIO + ' ' + ltrim(coalesce(replace(icarico,',0',''),'')) + replace(rtrim(substring(VEL,1,2)),'Z','W') 
,l.codca2=CASE WHEN STAGIONE='INVERNO' THEN 'N'
 WHEN STAGIONE='4 STAGIONI' THEN '4' 
ELSE 'E' END 
, l.desca2=CASE WHEN CODICESTAGIONE='I' THEN 'INVERNALI' 
 WHEN CODICESTAGIONE='4' THEN '4 STAGIONI'
ELSE 'ESTIVE' END ,
L.CODICERUMOROSITA = linky_integro_articoli.CODICERUMOROSITA
FROM 
LINKY.DBO.LINKY_EBAYARTICOLI L inner join
linky_integro_articoli on 
	l.codice=linky_integro_articoli.ipcodESTD
	and l.marca=linky_integro_articoli.marca 
	and l.partner in (select codicepartner from linky_partners where tipo='PNEUMATICI')
	and linky_integro_articoli.IMMAGINE IS NOT NULL 
	and linky_integro_articoli.immagine &lt;&gt; '' 
	and linky_integro_articoli.ebaycat&lt;&gt;''
	and bestprice=1 
-- inserita regola che determina il fatto che il prezzo di acquisto non può essre più basso di 20eur
and prezzoscontato > 20</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename />
      <sendOneStatement>F</sendOneStatement>
      <connection>LINKY</connection>
