    <connection>LINKY</connection>
    <sql>SELECT distinct
 codice as product_sku
, short_desc  as Titolo
, ebaycat
,'IT' as siteid
,'Italy' as country
,'FixedPriceItem' as Type
,  Marca 
, dbo.linky_calc_endprice(partner,'IT',case when category_path = 'CERCHI IN LEGA' then 'CERCHI' else 'ACCOPPIATE MOTO' end,'',prezzolistino)  as product_price
, product_in_stock as product_in_stock
, case when codca1 is not null then desca1 else '' end as diametro
, case when codca3 is not null then codca3 else '' end as spalla
, case when codca4 is not null then codca4 else '' end as larghezza
, case when codca5 is not null then desca5 else '' end as ivel
, case when codca6 is not null then desca6 else '' end as icarico
, case when codca2 is not null then codca2 else '' end as offset
, case when category_path = 'CERCHI IN LEGA' then 'CERCHI' 
else 'ACCOPPIATE MOTO' end as category_path
,product_full_image
FROM linky_ebayarticoli inner JOIN linky_ebay_report on linky_ebayarticoli.codice = linky_ebay_report.SKU and 'IT' = linky_ebay_report.SITEID
and product_in_stock&lt;&gt;qty

where product_in_stock > 0 and 
category_path  in ('ACCOPPIATE MOTO')
and (linky_ebayarticoli.siteid='Italy' or linky_ebayarticoli.siteid='IT')
and (partner is null or partner not in (SELECT CODICEPARTNER FROM LINKY_PARTNERS))</sql>
