    <sql>SELECT   
  l.codice AS IPCODE_EB
, l.marca AS MARCA_EB

, settore
, COALESCE(TITOLOALTERNATIVO,linky_integro_articoli.DESCRIZIONE,'') AS TITOLO
, linky_integro_articoli.marca AS Marchio
, LARGHEZZA 
, SERIE AS z_spalla
, ltrim(coalesce(replace(icarico,',0',''),'')) AS z_icarico
, VEL
, CODICECERCHIO AS Diametro
, linky_integro_articoli.radiale

FROM 
LINKY.DBO.LINKY_EBAYARTICOLI L inner join
linky_integro_articoli on 
	l.codice=linky_integro_articoli.SKU
--	and l.marca=linky_integro_articoli.marca 
	and l.partner in (select codicepartner from linky_partners where tipo='PNEUMATICI')
	and linky_integro_articoli.IMMAGINE IS NOT NULL 
	and linky_integro_articoli.immagine &lt;&gt; '' 
	and linky_integro_articoli.ebaycat&lt;&gt;''
	and bestprice=1 
-- inserita regola che determina il fatto che il prezzo di acquisto non può essre più basso di 20eur
and prezzoscontato > 20

--WHERE l.codice = '2730900'</sql>
