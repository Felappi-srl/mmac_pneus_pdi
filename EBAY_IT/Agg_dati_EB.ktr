        <jsScript_name>Script 1</jsScript_name>
        <jsScript_script>//Script qua


var tmp_SETTORE;
	tmp_SETTORE = trim(settore);

var tmp_codca5;
	tmp_codca5 = trim(VEL);






var R;

if (tmp_codca5 === '' || radiale == ''){
	R = ' R';
}

else if (tmp_SETTORE=='SCOOTER' &amp;&amp; tmp_codca5 == 'H'){
	R=' R';
} else if (tmp_SETTORE=='SCOOTER' &amp;&amp; tmp_codca5 != 'H'){
	R=' -';
}

else if (tmp_SETTORE=='MOTO' &amp;&amp; trim(radiale) == 'W'){
	R=' ZR';
}

else if (tmp_SETTORE=='MOTO' &amp;&amp; trim(radiale) == 'WX'){
	R=' ZR';
}

else if (tmp_SETTORE=='MOTO' &amp;&amp; indexOf(radiale,'_')>-1){
	R=' R';
}

else if (tmp_SETTORE=='MOTO' &amp;&amp; trim(radiale) == 'R'){
	R=' R';
}

else if (tmp_SETTORE=='MOTO' &amp;&amp; trim(radiale) == 'S'){
	R=' -';
}

else{
	R=' R';
}


var slash;
if ( trim(z_spalla).length > 0){
	slash='/';
}
else{
	slash= '';
}




var car;
if (trim(z_icarico) === ''){
	car='';
}

else if (tmp_SETTORE=='MOTO' &amp;&amp; trim(radiale) == 'W'){
	car='(' + trim(z_icarico) + tmp_codca5 + ')';

} 
else if (tmp_SETTORE=='MOTO' &amp;&amp; trim(radiale) == 'WX'){
	car=trim(z_icarico) + tmp_codca5.slice(0,1);
} 

else if ( tmp_SETTORE=='MOTO' &amp;&amp; trim(radiale) == 'R'){
	car=trim(z_icarico) + tmp_codca5;
}

else if ( tmp_SETTORE=='MOTO' &amp;&amp; trim(radiale) == 'S'){
	car=trim(z_icarico) + tmp_codca5;
}
else{
	car= trim(z_icarico) + tmp_codca5;
}



var short_desc;
short_desc = 'GOMME PNEUMATICI ' + trim(TITOLO) + ' ' + trim(LARGHEZZA) + slash + trim(z_spalla) + R + trim(Diametro) + ' ' + car +' ' + trim(Marchio);

var descrizione;
descrizione = trim(Marchio) + ' GOMME PNEUMATICI ' + trim(TITOLO) + ' ' + trim(LARGHEZZA) + slash +trim(z_spalla) + R + trim(Diametro) + ' ' + car;



var codca5;

if (tmp_codca5===''){
	codca5='';
}
else if (tmp_codca5=='WX'){
	codca5='W';
}
else if (indexOf(tmp_codca5,'_')>-1) {
	codca5=tmp_codca5.slice(0,1);
}
else if (tmp_codca5=='RA'){
	codca5='';
}
else {
	codca5=trim(VEL);
}


</jsScript_script>
