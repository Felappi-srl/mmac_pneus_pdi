    <entry>
      <name>CG Update stock</name>
      <description/>
      <type>SQL</type>
      <sql>UPDATE  L
SET 
l.prezzo=prezzoscontato , 
l.prezzolistino=prezzoscontato , 
l.product_publish='Y',
l.product_in_stock=	giacenza , 
l.product_available='12234563' , 
l.pubblicato=	'Y',
l.product_full_image= 'http://s3-eu-west-1.amazonaws.com/cdn.pneus-center.it/web/foto/' + ltrim(rtrim(lower(cast(immagine as varchar(max))))) + '.jpg'  
,l.product_thumb_image =  'http://s3-eu-west-1.amazonaws.com/cdn.pneus-center.it/web/foto/resized/' + ltrim(rtrim(lower(cast(immagine as varchar(max))))) + '.jpg' 
, l.CODCA1 =CODICECERCHIO
, l.DESCA1 =CODICECERCHIO
, l.CODCA2 =CASE WHEN CODICESTAGIONE='I' THEN 'N' ELSE NULL END
, l.DESCA2 =CASE WHEN CODICESTAGIONE='I' THEN 'INVERNALI' ELSE NULL END
, l.CODCA3 =SERIE
, l.DESCA3 ='SERIE ' + SERIE
, l.CODCA4 =LARGHEZZA
, l.DESCA4 =LARGHEZZA
, l.CODCA5 =replace(SUBSTRING(VEL,1,1),'Z','W')
, l.DESCA5 =replace(SUBSTRING(VEL,1,1),'Z','W') +': ' +SUBSTRING(VEL,2,4) + ' Km/h'
, l.CODCA6 =ltrim(coalesce(replace(icarico,',0',''),''))
, l.DESCA6 =ltrim(coalesce(replace(icarico,',0',''),''))

FROM 
LINKY.DBO.LINKY_EBAYARTICOLI L inner join
linky_integro_articoli on 
	l.codice=linky_integro_articoli.SKU 
	and l.marca=linky_integro_articoli.marca 
	and l.partner  in (SELECT CODICEPARTNER FROM LINKY_PARTNERS WHERE TIPO='PNEUMATICI' AND CODICEPARTNER&lt;&gt;'PN')
	and linky_integro_articoli.IMMAGINE IS NOT NULL 
	and linky_integro_articoli.immagine &lt;&gt; '' 
	and linky_integro_articoli.ebaycat&lt;&gt;''
	and bestprice=1</sql>
      <useVariableSubstitution>F</useVariableSubstitution>
      <sqlfromfile>F</sqlfromfile>
      <sqlfilename/>
      <sendOneStatement>F</sendOneStatement>
